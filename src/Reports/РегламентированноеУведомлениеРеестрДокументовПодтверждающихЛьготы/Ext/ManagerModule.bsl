Процедура ПроверкаОтчета_Форма2021_1(_ПредТипДог)
	Для Каждого Стр0 Из _ПредТипДог.Строки Цикл 
		Если Стр0.Строки.Количество() = 0 Тогда 
			Стр0.ОписаниеКА = "<Необходимо добавить контрагентов>";
			Стр0.ФлагОшибки = Истина;
		КонецЕсли;
		
		Для Каждого Стр1 Из Стр0.Строки Цикл 
			Если Стр1.Строки.Количество() = 0 Тогда 
				Стр1.ОписаниеКА = "<Необходимо добавить документы>";
				Стр1.ФлагОшибки = Истина;
			Иначе
				Если ЗначениеЗаполнено(Стр1.ИННЮЛ) 
					Или ЗначениеЗаполнено(Стр1.КПП)
					Или ЗначениеЗаполнено(Стр1.НаимОрг) Тогда 
					
					Если ЗначениеЗаполнено(Стр1.ИННЮЛ) 
						И Не РегламентированныеДанныеКлиентСервер.ИННСоответствуетТребованиям(Стр1.ИННЮЛ, Истина, "") Тогда 
						Стр1.ОписаниеКА = "<Некорректно заполнен ИНН>";
						Стр1.ФлагОшибки = Истина;
					ИначеЕсли ЗначениеЗаполнено(Стр1.КПП) 
						И Не РегламентированныеДанныеКлиентСервер.КППСоответствуетТребованиям(Стр1.КПП, "") Тогда 
						
						Стр1.ОписаниеКА = "<Некорректно заполнен КПП>";
						Стр1.ФлагОшибки = Истина;
					ИначеЕсли Не ЗначениеЗаполнено(Стр1.НаимОрг) Тогда  
						Стр1.ОписаниеКА = "<Не указано наименование организации>";
						Стр1.ФлагОшибки = Истина;
					КонецЕсли;
				Иначе
					Если Не ЗначениеЗаполнено(Стр1.Фамилия) Или Не ЗначениеЗаполнено(Стр1.Имя) Тогда
						Стр1.ОписаниеКА = "<Не указаны имя и фамилия>";
						Стр1.ФлагОшибки = Истина;
					ИначеЕсли ЗначениеЗаполнено(Стр1.ИННФЛ)
						И Не РегламентированныеДанныеКлиентСервер.ИННСоответствуетТребованиям(Стр1.ИННФЛ, Ложь, "") Тогда 
						
