				СтруктураПараметров.Вставить("Секции", СтрРазделить(ИменаСекций, ";", Ложь));
				
			КонецЕсли;
			
			СтруктураПараметров.Вставить("НомерСтраницы", НомерСтраницы);
			
			Если СтраницаОтчета.ИмяСтраницы = "Раздел8"
			 ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел8_Прил1"
			 ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел9"
			 ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел9_Прил1"
			 ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел10"
			 ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел11"
			 ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел12" Тогда
			 
			 Если НЕ ПечататьЭкраннуюФормуРазделов8_12 Тогда
				 
				 Если СтруктураРеквизитовФормы.мСохраненныйДок.ДанныеОтчета.Получить().УспешноеАвтозаполнение = Неопределено Тогда
					 ВывестиНаПечатьЛистыРазделов8_12_ЗаполненныхБезДеленияНаСекции(
					 ИмяФормы, СтруктураПараметров, ОбщиеРеквизитыРазделов, СтруктураРеквизитовФормы);
				 Иначе
					 ВывестиНаПечатьЛистыРазделов8_12_ЗаполненныхСДелениемНаСекции(
					 ИмяФормы, СтруктураПараметров, ОбщиеРеквизитыРазделов, СтруктураРеквизитовФормы, СтраницыРазделов8_12);
				 КонецЕсли;
				 
			 КонецЕсли;
				
			ИначеЕсли СтраницаОтчета.ИмяСтраницы = "Раздел2"
				  ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел3_Прил1"
				  ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел5" Тогда
				
				ДанныеРазделаДляПечати = Новый Структура;
				
				Для Каждого Страница Из ДанныеРазделов[СтраницаОтчета.ИмяСтраницы] Цикл
					
					Если ТипЗнч(Страница.Данные) = Тип("СписокЗначений") Тогда
						
						ДанныеРазделаДляПечати.Вставить(СтраницаОтчета.ИмяСтраницы, Страница.Данные[0].Значение);
						
					Иначе
						
						ДанныеРазделаДляПечати.Вставить(СтраницаОтчета.ИмяСтраницы, Страница.Данные);
						
					КонецЕсли;
					
					Если СтраницаОтчета.ИмяСтраницы = "Раздел5"
					   И ТипЗнч(Страница.ДанныеДопСтрок) = Тип("СписокЗначений") Тогда
						
						ДанныеРазделаДляПечати.Вставить(СтруктураРеквизитовФормы.мИдГруппы5, 
							ДанныеФормыВЗначение(Страница.ДанныеДопСтрок[0].Значение, Тип("ТаблицаЗначений")));
						
					ИначеЕсли СтраницаОтчета.ИмяСтраницы = "Раздел5" Тогда
						
						ДанныеРазделаДляПечати.Вставить(СтруктураРеквизитовФормы.мИдГруппы5, Страница.ДанныеДопСтрок);
						
					КонецЕсли;
					
					ВывестиНаПечатьЛист(ИмяФормы,
					СтруктураПараметров,
					ОбщиеРеквизитыРазделов,
					СтруктураРеквизитовФормы,
					ДанныеРазделаДляПечати,
					ДанныеРазделов[СтраницаОтчета.ИмяСтраницы].Индекс(Страница) + 1);
					
				КонецЦикла;
				
			Иначе
				
				ВывестиНаПечатьЛист(ИмяФормы,
				СтруктураПараметров,
				ОбщиеРеквизитыРазделов,
				СтруктураРеквизитовФормы,
				ДанныеРазделов);
				
			КонецЕсли;
		
			СтруктураПараметров.Свойство("НомерСтраницы", НомерСтраницы); 
			
		КонецЦикла;
		
		ЗафиксироватьТранзакцию();
		
	Исключение
		
		ОтменитьТранзакцию();
		
		ЗаписьЖурналаРегистрации(НСтр("ru = 'Выполнение операции'"),
		УровеньЖурналаРегистрации.Ошибка,
		,
		,
		ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
		
	КонецПопытки;
	
	Если КоличествоВыбранныхСтраниц > 0 Тогда
		
		ДеревоВыбранныхСтраниц.Строки.Сдвинуть(КоличествоВыбранныхСтраниц - 1, 1 - КоличествоВыбранныхСтраниц);
		
	КонецЕсли;
	
	Если НЕ ПечатныйБланкФормируетсяВФорматеPDF Тогда
		Возврат "";
	КонецЕсли;
	
	ПакетОтображаемыхДокументов = Новый ПакетОтображаемыхДокументов;
	
	СписокПечатаемыхЛистов = СтруктураРеквизитовФормы.мПечатныеФормы;
	
	Если СписокПечатаемыхЛистов.Количество() > 0
	   И ВРег(СокрЛП(СписокПечатаемыхЛистов[0].Представление)) = ВРег(СокрЛП("СсылкаНаРегламентированныйОтчет")) Тогда
		
		УникальныеИдентификаторы = СписокПечатаемыхЛистов.ВыгрузитьЗначения();
		УникальныеИдентификаторы.Удалить(0);
		
		СсылкаНаРегламентированныйОтчет = СписокПечатаемыхЛистов.Получить(0);
		
		СписокПечатаемыхЛистов.Очистить();
		СписокПечатаемыхЛистов.Добавить(СсылкаНаРегламентированныйОтчет.Значение,
										СсылкаНаРегламентированныйОтчет.Представление);
				
		Запрос = Новый Запрос;
		
		ТекстЗапроса = "ВЫБРАТЬ РАЗРЕШЕННЫЕ
		               |	ПечатныеБланкиРегламентированныхОтчетов.ПечатныйБланк КАК ПечатныйБланк,
		               |	ПечатныеБланкиРегламентированныхОтчетов.НомерСтраницы КАК НомерСтраницы
		               |ИЗ
		               |	РегистрСведений.ПечатныеБланкиРегламентированныхОтчетов КАК ПечатныеБланкиРегламентированныхОтчетов
		               |ГДЕ
		               |	ПечатныеБланкиРегламентированныхОтчетов.РегламентированныйОтчет = &РегламентированныйОтчет
		               |	И ПечатныеБланкиРегламентированныхОтчетов.УникальныйИдентификатор В(&УникальныеИдентификаторы)
		               |УПОРЯДОЧИТЬ ПО
		               |	НомерСтраницы";
		
		Запрос.Текст = ТекстЗапроса;
										
		Запрос.УстановитьПараметр("РегламентированныйОтчет", СписокПечатаемыхЛистов[0].Значение);
		Запрос.УстановитьПараметр("УникальныеИдентификаторы", УникальныеИдентификаторы);
				
		РезультатЗапроса = Запрос.Выполнить();
		
		Если НЕ РезультатЗапроса.Пустой() Тогда
			
			Выборка = РезультатЗапроса.Выбрать();
			
			Пока Выборка.Следующий() Цикл
				
				ПакетОтображаемыхДокументов.Состав.Добавить(
					ПоместитьВоВременноеХранилище(Выборка.ПечатныйБланк.Получить(), Новый УникальныйИдентификатор));
				
			КонецЦикла;
			
		КонецЕсли;
		
		Если ПечататьЭкраннуюФормуРазделов8_12 Тогда
			
			Для Каждого СтраницаОтчета Из ДеревоВыбранныхСтраниц.Строки Цикл
				
				Если СтраницаОтчета.ВыводНаПечать = 0 Тогда
					
					Продолжить;
					
				КонецЕсли;
				
				Если СтраницаОтчета.ИмяСтраницы = "Раздел8"
				 ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел8_Прил1"
				 ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел9"
				 ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел9_Прил1"
				 ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел10"
				 ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел11"
				 ИЛИ СтраницаОтчета.ИмяСтраницы = "Раздел12" Тогда
					
					Секции = СписокСекцийРазделаИзРегистра(ИмяФормы,
														   СтраницаОтчета.ИмяСтраницы,
														   СтраницыРазделов8_12,
														   СтруктураРеквизитовФормы);
					
					Если Секции.Количество() > 0 Тогда
						
						Для Каждого Секция Из Секции Цикл
						
							НомерПервойСтрокиСегмента = Секция.НомерПервойСтроки;
						
							ТабличныйДокументРаздела = РегламентированнаяОтчетность.СегментТабличногоДокументаРазделаДекларацииНДС(
								СтруктураРеквизитовФормы.мСохраненныйДок, СтраницаОтчета.ИмяСтраницы, НомерПервойСтрокиСегмента);
							
							ТабличныйДокументРаздела.ОриентацияСтраницы = ОриентацияСтраницы.Ландшафт;
						
							ТабличныйДокументРаздела.ПолеСверху = 5;
							ТабличныйДокументРаздела.ПолеСправа = 5;
							ТабличныйДокументРаздела.ПолеСнизу  = 5;
							ТабличныйДокументРаздела.ПолеСлева  = 5;
						
							ТабличныйДокументРаздела.ЧерноБелаяПечать = Истина;
							ТабличныйДокументРаздела.ЧерноБелыйПросмотр = Истина;
							
							ТабличныйДокументРаздела.ВерхнийКолонтитул.Выводить = Ложь;
							ТабличныйДокументРаздела.НижнийКолонтитул.Выводить = Ложь;
							
							ТабличныйДокументРаздела.АвтоМасштаб = Истина;
							
							ПакетОтображаемыхДокументов.Состав.Добавить(
								ПоместитьВоВременноеХранилище(ТабличныйДокументРаздела, Новый УникальныйИдентификатор));
							
						КонецЦикла;
						
					КонецЕсли;
					
				КонецЕсли;
					
			КонецЦикла;
				
		КонецЕсли;
	
	КонецЕсли;
	
	ИмяВременногоФайла = ПолучитьИмяВременногоФайла("pdf");
	
	ПакетОтображаемыхДокументов.ЗаписатьФайлДляПечати(ИмяВременногоФайла);
	
	ФайлПечатногоБланка = Новый ДвоичныеДанные(ИмяВременногоФайла);
	
	АдресВоВременномХранилище = ПоместитьВоВременноеХранилище(ФайлПечатногоБланка, Новый УникальныйИдентификатор);
	
	УдалитьФайлы(ИмяВременногоФайла);
	
	ИмяФайлаПечатногоБланка = СтруктураРеквизитовФормы.НаименованиеОтчета
							+ НСтр("ru=' за '")
							+ ПредставлениеПериода(
								НачалоДня(СтруктураРеквизитовФормы.мСохраненныйДок.ДатаНачала),
							 	КонецДня(СтруктураРеквизитовФормы.мСохраненныйДок.ДатаОкончания),
							 	"ФП = Истина")
							+ " (" + СтруктураРеквизитовФормы.мСохраненныйДок.Организация + ").pdf";
	
	ПечатныйБланк = Новый Структура;
	ПечатныйБланк.Вставить("АдресПечатногоБланка", АдресВоВременномХранилище);
	ПечатныйБланк.Вставить("ИмяФайлаПечатногоБланка",
		ОбщегоНазначенияКлиентСервер.ЗаменитьНедопустимыеСимволыВИмениФайла(ИмяФайлаПечатногоБланка));
	
	Возврат ПечатныйБланк;
	
КонецФункции

Функция ПолучитьОбщиеРеквизитыСТитульногоЛиста(ИмяФормы, СтруктураДанныхТитульный)
	
	МассивИНН         = Новый Массив(12);
	МассивКПП         = Новый Массив(9);
	МассивДатаПодписи = Новый Массив(8);
	
	СтрИНН = "";
	
	Если СтруктураДанныхТитульный.Свойство("ИНН1", СтрИНН) Тогда
		
		Если СтрДлина(СтрИНН) = 10 Тогда
			
			СтрИНН = СтрИНН + "--";
			
		КонецЕсли;
		
		Для Ном = 1 По СтрДлина(СтрИНН) Цикл
			
			МассивИНН[Ном - 1] = Сред(СтрИНН, Ном, 1);
			
		КонецЦикла;
	
	КонецЕсли;

	СтрКПП = "";
	
	Если СтруктураДанныхТитульный.Свойство("КПП1", СтрКПП) Тогда
						
		Для Ном = 1 По СтрДлина(СтрКПП) Цикл
			
			МассивКПП[Ном - 1] = Сред(СтрКПП, Ном, 1);
			
		КонецЦикла;
		
	КонецЕсли;

	ДатаПодписи = "";
	
	Если СтруктураДанныхТитульный.Свойство("ДатаПодписи", ДатаПодписи) Тогда
		
		Если ЗначениеЗаполнено(ДатаПодписи) Тогда
			
			Если ТипЗнч(ДатаПодписи) = Тип("Строка") Тогда 
				
				СтрДатаПодписи = Лев(СтрЗаменить(ДатаПодписи, ".", ""), 8);
				
			ИначеЕсли ТипЗнч(ДатаПодписи) = Тип("Дата") Тогда 
				
				СтрДатаПодписи = Лев(СтрЗаменить(Формат(ДатаПодписи, "ДФ = ""дд.ММ.гггг 'г.'"""), ".", ""), 8);
				
			КонецЕсли;
			
		Иначе
			
			СтрДатаПодписи = "        ";
			
		КонецЕсли;
		
		Для Ном = 1 По СтрДлина(СтрДатаПодписи) Цикл
			
			МассивДатаПодписи[Ном - 1] = Сред(СтрДатаПодписи, Ном, 1);
			
		КонецЦикла;
		
	КонецЕсли;

	ОбщиеРеквизитыРазделов = Новый Структура;
	
	ОбщиеРеквизитыРазделов.Вставить("ИНН", МассивИНН);
	ОбщиеРеквизитыРазделов.Вставить("КПП", МассивКПП);
	ОбщиеРеквизитыРазделов.Вставить("ДатаПодписи", МассивДатаПодписи);
	
	Возврат ОбщиеРеквизитыРазделов;
	
КонецФункции

Процедура ВывестиНаПечатьЛистыРазделов8_12_ЗаполненныхБезДеленияНаСекции(ИмяФормы,
																		 СтруктураПараметров,
																		 ОбщиеРеквизитыРазделов,
																		 СтруктураРеквизитовФормы)
	
	Если НЕ СтруктураРеквизитовФормы.Свойство("АдресДанных" + СтруктураПараметров.ИмяСтраницы) Тогда
		Данные = Неопределено;
	ИначеЕсли СтруктураРеквизитовФормы["АдресДанных" + СтруктураПараметров.ИмяСтраницы] = Неопределено Тогда
		Данные = Неопределено;
	Иначе
		Данные = ПолучитьИзВременногоХранилища(СтруктураРеквизитовФормы["АдресДанных" + СтруктураПараметров.ИмяСтраницы]);
		Если Данные.Записи.Количество() = 0 Тогда
			Данные = Неопределено;
		КонецЕсли;
	КонецЕсли;
	
	Если Данные = Неопределено Тогда
		
		Данные = ДанныеДляПечатиПустыхМЧБРазделов8_12(ИмяФормы);
		
	КонецЕсли;
	
	Данные.Вставить("ЭтоПерваяСтраницаРаздела",    Неопределено);
	Данные.Вставить("ЭтоПоследняяСтраницаРаздела", Неопределено);
	
	ВывестиНаПечатьЛистыРазделов8_12_ОбщаяЧасть(
		ИмяФормы, СтруктураПараметров, ОбщиеРеквизитыРазделов, Данные, СтруктураРеквизитовФормы);
	
КонецПроцедуры

Процедура ВывестиНаПечатьЛистыРазделов8_12_ЗаполненныхСДелениемНаСекции(ИмяФормы,
																		СтруктураПараметров,
																		ОбщиеРеквизитыРазделов,
																		СтруктураРеквизитовФормы,
																		СтраницыРазделов8_12)
	
	ДанныеРаздела = ДанныеРазделаНДСДляПечати(
		ИмяФормы, СтруктураПараметров.ИмяСтраницы, СтруктураРеквизитовФормы, СтраницыРазделов8_12);
		
	Если ДанныеРаздела = Неопределено Тогда
		
		Данные = ДанныеДляПечатиПустыхМЧБРазделов8_12(ИмяФормы);
		
		ВывестиНаПечатьЛистыРазделов8_12_ОбщаяЧасть(
			ИмяФормы, СтруктураПараметров, ОбщиеРеквизитыРазделов, Данные, СтруктураРеквизитовФормы);
		
	Иначе
		
		СтрокиРаздела = ДанныеРаздела.СтрокиРаздела;
		
		ПечататьОдинКомплектЛистовРаздела
			= (СтруктураРеквизитовФормы["АктуальностьСведений" + СтруктураПараметров.ИмяСтраницы] = 1
			И СтруктураРеквизитовФормы.ВидДокумента <> 0);
		
		Для Каждого НомерПервойСтрокиСекции Из СтрокиРаздела Цикл
			
			СтрокиСегментаРаздела = РегламентированнаяОтчетность.СегментДанныхРазделаДекларацииНДС(
				СтруктураРеквизитовФормы.мСохраненныйДок, СтруктураПараметров.ИмяСтраницы, НомерПервойСтрокиСекции);
			
			Для Каждого СтрокаДанных ИЗ СтрокиСегментаРаздела Цикл
				
				// Преобразование структуры данных, передаваемой в общий механизм вывода листов
				// МЧБ разделов 8 - 12 при новом ("сегментированном") варианте хранения данных.
				Данные = Новый Структура;
				
				МассивКопируемыхСтрок = Новый Массив;
				МассивКопируемыхСтрок.Добавить(СтрокаДанных);
				
				// Данные, полученные из ИБ потребителя.
				Данные.Вставить("Записи", СтрокиСегментаРаздела.Скопировать(МассивКопируемыхСтрок));
				Данные.Вставить("Итоги",  ДанныеРаздела.Итоги);
				
				// Служебные данные, определяющие необходимость вывода на печать
				// значений отдельных показателей и/или формат вывода этих значений.
				Данные.Вставить("ЭтоПерваяСтраницаРаздела",
					НомерПервойСтрокиСекции = СтрокиРаздела[0] И СтрокиСегментаРаздела.Индекс(СтрокаДанных) = 0);
				Данные.Вставить("ЭтоПоследняяСтраницаРаздела",
					НомерПервойСтрокиСекции = СтрокиРаздела[СтрокиРаздела.Количество() - 1]
					И СтрокиСегментаРаздела.Индекс(СтрокаДанных) = СтрокиСегментаРаздела.Количество() - 1);
				
				ВывестиНаПечатьЛистыРазделов8_12_ОбщаяЧасть(
