&НаСервере
Функция КоличествоСтрок(Данные, Показатель)
	
	Инд = 1; 
	Количество = 0;
	Показатель_ = СтрЗаменить(Показатель, "%1", "");
	
	Пока Данные.Свойство(Показатель_ + ?(Инд < 1000, Инд, XMLСтрока(Инд))) Цикл
		Количество = Количество + 1;
		Инд = Инд + 1;
	КонецЦикла;
	
	Возврат Количество;
	
КонецФункции

