	ВнестиПоказательВРасшифровку(Р1Прил2Ст20Гр02, "Раздел1Прил2");
	
	КС = ПоказателиКС(Р1Прил2Ст20Гр01.Значение, Р1Прил2Ст20Гр01ПРП.Значение + Р1Прил2Ст20Гр02.Значение);
