	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>ВЫБРАТЬ РАЗЛИЧНЫЕ
	ЖурналДокументовДвиженияТоваров.Ссылка КАК Ссылка,
	ЖурналДокументовДвиженияТоваров.Дата КАК Дата,
	ЖурналДокументовДвиженияТоваров.ПометкаУдаления КАК ПометкаУдаления,
	ВЫБОР
		КОГДА ПОДСТРОКА(ЖурналДокументовДвиженияТоваров.Номер, 1, 10) = "0000000000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ЖурналДокументовДвиженияТоваров.Номер, 11, 1)
		КОГДА ПОДСТРОКА(ЖурналДокументовДвиженияТоваров.Номер, 1, 9) = "000000000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ЖурналДокументовДвиженияТоваров.Номер, 10, 2)
		КОГДА ПОДСТРОКА(ЖурналДокументовДвиженияТоваров.Номер, 1, 8) = "00000000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ЖурналДокументовДвиженияТоваров.Номер, 9, 3)
		КОГДА ПОДСТРОКА(ЖурналДокументовДвиженияТоваров.Номер, 1, 7) = "0000000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ЖурналДокументовДвиженияТоваров.Номер, 8, 4)
		КОГДА ПОДСТРОКА(ЖурналДокументовДвиженияТоваров.Номер, 1, 6) = "000000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ЖурналДокументовДвиженияТоваров.Номер, 7, 5)
		КОГДА ПОДСТРОКА(ЖурналДокументовДвиженияТоваров.Номер, 1, 5) = "00000"
