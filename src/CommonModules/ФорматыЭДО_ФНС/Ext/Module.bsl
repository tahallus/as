		КонецЕсли;
	КонецЕсли;
	
	Данные.Вставить("КодОКПО", РаботаСФайламиБЭД.ЗначениеСвойстваXDTO(СведенияОбУчастнике, "ОКПО"));
	Данные.Вставить("СтруктурноеПодразделение", РаботаСФайламиБЭД.ЗначениеСвойстваXDTO(СведенияОбУчастнике, "СтруктПодр"));
