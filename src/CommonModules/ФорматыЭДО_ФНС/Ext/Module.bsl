	КонецЕсли;

	РаботаСФайламиБЭД.ЗаполнитьСвойствоXDTO(Документ, "СодФХЖ2", СодФХЖ2, , Ошибки);
	Документ.ИнфДопСв.Добавить("1"); // К документу дополнительная информация не составляется.
		
		// Сведения о лице, подписывающем файл обмена.
