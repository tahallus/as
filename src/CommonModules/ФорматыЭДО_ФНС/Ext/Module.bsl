			ФИО.Вставить("Имя",      РаботаСФайламиБЭД.ЗначениеСвойстваXDTO(Подписант, "ИП.ФИО.Имя"     ,, "Строка"));
			ФИО.Вставить("Отчество", РаботаСФайламиБЭД.ЗначениеСвойстваXDTO(Подписант, "ИП.ФИО.Отчество",, "Строка"));
			
