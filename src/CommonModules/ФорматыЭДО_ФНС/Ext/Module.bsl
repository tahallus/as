			КодСтраны = ИнформацияОбАдресе.КодСтр;
			
		КонецЕсли;
		
		Если КодСтраны = "643" Тогда
			
			ЗаполнитьРеквизитДерева(ДеревоДанных, ВидУчастника + ".Адрес.Произвольный",
