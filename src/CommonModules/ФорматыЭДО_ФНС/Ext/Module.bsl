	ИмяИЗначение = Новый Структура;
	ИмяИЗначение.Вставить("Имя", Имя);
	ИмяИЗначение.Вставить("Значение", Значение);
	
	СуммаРазн.Добавить(ИмяИЗначение);
	
КонецПроцедуры


