	СпособОбработкиПоступлениеДопРасходов = ВРег("ДополнительныеРасходы");
	Если ТипЗнч(СпособОбработки) = Тип("Структура") Тогда
		Результат = (ВРег(СпособОбработки.ПервичныйДокумент) = СпособОбработкиПоступлениеДопРасходов);
	Иначе
		Результат = (ВРег(СпособОбработки) = СпособОбработкиПоступлениеДопРасходов);
	КонецЕсли;
	
	Возврат Результат;
	
КонецФункции

Функция НакладнаяДляДопРасходов(Контрагент, ДатаДокумента) Экспорт
	
	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ ПЕРВЫЕ 1
	|	ПриходнаяНакладнаяЗапасы.Ссылка КАК Ссылка
	|ИЗ
	|	Документ.ПриходнаяНакладная.Запасы КАК ПриходнаяНакладнаяЗапасы
	|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ЭлектронныйДокументВходящийЭДО.УдалитьДокументыОснования КАК ЭлектронныйДокументВходящийДокументыОснования
	|		ПО ПриходнаяНакладнаяЗапасы.Ссылка = ЭлектронныйДокументВходящийДокументыОснования.ДокументОснование
	|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ДополнительныеРасходы.Запасы КАК ДополнительныеРасходыЗапасы
	|		ПО ПриходнаяНакладнаяЗапасы.Ссылка = ДополнительныеРасходыЗапасы.ДокументПоступления
	|			И (НЕ ДополнительныеРасходыЗапасы.Ссылка.ПометкаУдаления)
	|ГДЕ
	|	НЕ ПриходнаяНакладнаяЗапасы.Ссылка.ПометкаУдаления
	|	И ПриходнаяНакладнаяЗапасы.Ссылка.Контрагент = &Контрагент
	|	И ДополнительныеРасходыЗапасы.Ссылка ЕСТЬ NULL
	|	И ПриходнаяНакладнаяЗапасы.Ссылка.Дата >= &НачалоПериода
	|
	|УПОРЯДОЧИТЬ ПО
	|	ПриходнаяНакладнаяЗапасы.Ссылка.Дата УБЫВ";
	
	Запрос.УстановитьПараметр("Контрагент", Контрагент);
	Запрос.УстановитьПараметр("НачалоПериода", ДатаДокумента - 86400*30);
	
	Выборка = Запрос.Выполнить().Выбрать();
	Если Выборка.Следующий() Тогда
		Возврат Выборка.Ссылка;
	КонецЕсли;
	
КонецФункции

