Процедура СоздатьГруппыДляНедостающихПрофилей(ПараметрыОбработчика) Экспорт

	Если ОбщегоНазначения.ЭтоПодчиненныйУзелРИБ() Тогда
		Возврат;
	КонецЕсли;

	РодительДополнительныхГруппДоступа = РодительДополнительныхГруппДоступа();
	ИдентификаторыОсновныхПрофилей = ИдентификаторыОсновныхПрофилей();

	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ ПЕРВЫЕ 500
	|	ПрофилиГруппДоступа.Ссылка,
	|	ПрофилиГруппДоступа.Наименование,
	|	ПрофилиГруппДоступа.ИдентификаторПоставляемыхДанных,
	|	ПрофилиГруппДоступа.ВидыДоступа.(
	|		ВидДоступа,
	|		ВсеРазрешены),
	|	ПрофилиГруппДоступа.ЗначенияДоступа.(
	|		ВидДоступа,
	|		ЗначениеДоступа,
	|		ВключаяНижестоящие)
	|ИЗ
	|	Справочник.ПрофилиГруппДоступа КАК ПрофилиГруппДоступа
	|		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ГруппыДоступа КАК ГруппыДоступа
	|		ПО ПрофилиГруппДоступа.Ссылка = ГруппыДоступа.Профиль
	|ГДЕ
	|	ГруппыДоступа.Ссылка ЕСТЬ NULL
	|	И НЕ ПрофилиГруппДоступа.ЭтоГруппа";

	РезультатЗапроса = Запрос.Выполнить();
	Если РезультатЗапроса.Пустой() Тогда
		Возврат;
	КонецЕсли;

	ОшибкиОтсутствуют = Истина;
	Выборка = РезультатЗапроса.Выбрать();

	Пока Выборка.Следующий() Цикл

		НоваяГруппа = Справочники.ГруппыДоступа.СоздатьЭлемент();
		НоваяГруппа.Наименование = Выборка.Наименование;
		Если ИдентификаторыОсновныхПрофилей[Выборка.ИдентификаторПоставляемыхДанных] = Неопределено Тогда
			НоваяГруппа.Родитель = РодительДополнительныхГруппДоступа;
		КонецЕсли;
		НоваяГруппа.Пользователь = Неопределено;
		НоваяГруппа.Профиль = Выборка.Ссылка;
		НоваяГруппа.Ответственный = Справочники.Пользователи.ПустаяСсылка();
		НоваяГруппа.Комментарий = Справочники.ПрофилиГруппДоступа.ПояснениеПоставляемогоПрофиля(Выборка.Ссылка);

		ВыборкаВидыДоступа = Выборка.ВидыДоступа.Выбрать();
		Пока ВыборкаВидыДоступа.Следующий() Цикл
			ЗаполнитьЗначенияСвойств(НоваяГруппа.ВидыДоступа.Добавить(), ВыборкаВидыДоступа);
		КонецЦикла;

		ВыборкаЗначенияДоступа = Выборка.ЗначенияДоступа.Выбрать();
		Пока ВыборкаЗначенияДоступа.Следующий() Цикл
			ЗаполнитьЗначенияСвойств(НоваяГруппа.ЗначенияДоступа.Добавить(), ВыборкаЗначенияДоступа);
		КонецЦикла;

		Попытка
			НоваяГруппа.Записать();
		Исключение
			ПодробноеПредставлениеОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
			ОшибкиОтсутствуют = Ложь;
			ЗаписьЖурналаРегистрации(НСтр(
					"ru = 'УправлениеДоступомУНФ.СоздатьГруппыДляНедостающихПрофилей'",
				ОбщегоНазначения.КодОсновногоЯзыка()), УровеньЖурналаРегистрации.Ошибка,
				Метаданные.Справочники.ГруппыДоступа, , ПодробноеПредставлениеОшибки);
		КонецПопытки;

	КонецЦикла;

	ПараметрыОбработчика.ОбработкаЗавершена = ОшибкиОтсутствуют И Выборка.Количество() < 500;

КонецПроцедуры

Процедура ОбработатьГруппыДоступаПередЗаписью(Источник, Отказ) Экспорт

	Если Источник.ОбменДанными.Загрузка = Истина Тогда
		Возврат;
	КонецЕсли;

	Если Источник.ПометкаУдаления Тогда
		Возврат;
	КонецЕсли;

	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	СтруктурныеЕдиницы.Наименование КАК НаименованиеПодразделения
	|ИЗ
	|	Справочник.СтруктурныеЕдиницы КАК СтруктурныеЕдиницы
	|ГДЕ
	|	СтруктурныеЕдиницы.Ссылка В (&ЗначенияДоступа)
	|	И ТипСтруктурнойЕдиницы = ЗНАЧЕНИЕ(Перечисление.ТипыСтруктурныхЕдиниц.Подразделение)
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	СтруктурныеЕдиницы.Ссылка КАК МагазинГруппаСкладов
	|ИЗ
	|	Справочник.СтруктурныеЕдиницы КАК СтруктурныеЕдиницы
	|ГДЕ
	|	СтруктурныеЕдиницы.Ссылка В (&ЗначенияДоступа)
	|	И ТипСтруктурнойЕдиницы = ЗНАЧЕНИЕ(Перечисление.ТипыСтруктурныхЕдиниц.МагазинГруппаСкладов)";
	Запрос.УстановитьПараметр("ЗначенияДоступа", Источник.ЗначенияДоступа.ВыгрузитьКолонку("ЗначениеДоступа"));
	РезультатыЗапроса = Запрос.ВыполнитьПакет();
	Если Не РезультатыЗапроса[0].Пустой() Тогда
		НаименованияПодразделений = РезультатыЗапроса[0].Выгрузить().ВыгрузитьКолонку("НаименованиеПодразделения");
		ПредставленияПодразделений = СтрСоединить(НаименованияПодразделений, ", ");
		ТекстСообщения = СтрШаблон(
			НСтр("ru = 'Ограничение доступа возможно только по складам и магазинам.
				 |Исключите следующие подразделения из ограничений: %1'"), ПредставленияПодразделений);
		ОбщегоНазначения.СообщитьПользователю(ТекстСообщения, , , , Отказ);
		Возврат;
	КонецЕсли;

	Если Не РезультатыЗапроса[1].Пустой() Тогда
		Выборка = РезультатыЗапроса[1].Выбрать();
		Пока Выборка.Следующий() Цикл
			ОтборСтрок = Новый Структура("ЗначениеДоступа", Выборка.МагазинГруппаСкладов);
			Для Каждого НайденнаяСтрока Из Источник.ЗначенияДоступа.НайтиСтроки(ОтборСтрок) Цикл
				НайденнаяСтрока.ВключаяНижестоящие = Истина;
			КонецЦикла;
		КонецЦикла;
	КонецЕсли;

КонецПроцедуры

Процедура ПроверитьГруппыДоступаПриЗаписи(Источник, Отказ) Экспорт

	Если Источник.ОбменДанными.Загрузка = Истина Тогда
		Возврат;
	КонецЕсли;

	Если Источник.ПометкаУдаления Тогда
		Возврат;
	КонецЕсли;

	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ
	|	ГруппыДоступаПользователи.Пользователь КАК Пользователь,
	|	Представление(ГруппыДоступаПользователи.Пользователь) КАК ПредставлениеПользователя
	|ПОМЕСТИТЬ ВТ_ПроверяемыеПользователи
	|ИЗ
	|	Справочник.ГруппыДоступа.Пользователи КАК ГруппыДоступаПользователи
	|ГДЕ
	|	ГруппыДоступаПользователи.Ссылка = &Ссылка
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ГруппыДоступаПользователи.Ссылка КАК Ссылка,
	|	ГруппыДоступаПользователи.Пользователь КАК Пользователь,
	|	ГруппыДоступаПользователи.Ссылка.Наименование КАК НаименованиеГруппы
	|ПОМЕСТИТЬ ВТ_ГруппыСПрофилямиРМК
	|ИЗ
	|	ВТ_ПроверяемыеПользователи КАК ВТ_ПроверяемыеПользователи
	|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ГруппыДоступа.Пользователи КАК ГруппыДоступаПользователи
	|		ПО ВТ_ПроверяемыеПользователи.Пользователь = ГруппыДоступаПользователи.Пользователь
	|ГДЕ
	|	ГруппыДоступаПользователи.Ссылка.Профиль.ИдентификаторПоставляемыхДанных В (&ИдентификаторыРМК)
	|	И НЕ ГруппыДоступаПользователи.Ссылка.ПометкаУдаления
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ГруппыДоступаПользователи.Ссылка КАК Ссылка,
	|	ГруппыДоступаПользователи.Пользователь КАК Пользователь,
	|	ГруппыДоступаПользователи.Ссылка.Наименование КАК НаименованиеГруппы
	|ПОМЕСТИТЬ ВТ_ГруппыСПрочимиПрофилями
	|ИЗ
	|	ВТ_ПроверяемыеПользователи КАК ВТ_ПроверяемыеПользователи
	|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ГруппыДоступа.Пользователи КАК ГруппыДоступаПользователи
	|		ПО ВТ_ПроверяемыеПользователи.Пользователь = ГруппыДоступаПользователи.Пользователь
	|ГДЕ
	|	ГруппыДоступаПользователи.Ссылка.Профиль.ИдентификаторПоставляемыхДанных НЕ В (&ИдентификаторыРМК)
	|	И НЕ ГруппыДоступаПользователи.Ссылка.ПометкаУдаления
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ВТ_ПроверяемыеПользователи.ПредставлениеПользователя КАК ПредставлениеПользователя,
	|	ВТ_ГруппыСПрофилямиРМК.НаименованиеГруппы КАК ГруппаСПрофилямиРМК,
	|	ВТ_ГруппыСПрочимиПрофилями.НаименованиеГруппы КАК ГруппаСПрочимиПрофилями
	|ИЗ
	|	ВТ_ПроверяемыеПользователи КАК ВТ_ПроверяемыеПользователи
	|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_ГруппыСПрофилямиРМК КАК ВТ_ГруппыСПрофилямиРМК
	|		ПО ВТ_ПроверяемыеПользователи.Пользователь = ВТ_ГруппыСПрофилямиРМК.Пользователь
	|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_ГруппыСПрочимиПрофилями КАК ВТ_ГруппыСПрочимиПрофилями
	|		ПО ВТ_ПроверяемыеПользователи.Пользователь = ВТ_ГруппыСПрочимиПрофилями.Пользователь";
	ИдентификаторыРМК = Новый Массив;
	ИдентификаторыРМК.Добавить(ПрофильРабочееМестоСтаршегоКассира().УИД);
	ИдентификаторыРМК.Добавить(ПрофильРабочееМестоОперационногоКассира().УИД);
	Запрос.УстановитьПараметр("ИдентификаторыРМК", ИдентификаторыРМК);
	Запрос.УстановитьПараметр("Ссылка", Источник.Ссылка);

	РезультатЗапроса = Запрос.Выполнить();
	Если РезультатЗапроса.Пустой() Тогда
		Возврат;
	КонецЕсли;

	СтрокиСообщения = Новый Массив;

	Выборка = РезультатЗапроса.Выбрать();
	Пока Выборка.Следующий() Цикл
		СтрокаСообщения = СтрШаблон(НСтр(
			"ru = 'Пользователь ""%1"" не может одновременно состоять в группе ""%2"" и ""%3"".'"),
			Выборка.ПредставлениеПользователя, Выборка.ГруппаСПрофилямиРМК, Выборка.ГруппаСПрочимиПрофилями);
		СтрокиСообщения.Добавить(СтрокаСообщения);
	КонецЦикла;

	ТекстСообщения = СтрСоединить(СтрокиСообщения, Символы.ПС);
	ВызватьИсключение ТекстСообщения;

КонецПроцедуры

Процедура ПредоставитьПользователямДоступКРедактированиюБанковскихСчетов(ПараметрыОбработчика) Экспорт

	Если ОбщегоНазначения.ЭтоПодчиненныйУзелРИБ() Тогда
		Возврат;
	КонецЕсли;

	СсылкаНаПрофильРедактированиеБанковскихСчетов = УправлениеДоступом.ПоставляемыйПрофильПоИдентификатору(
		ПрофильРедактированиеБанковскихСчетов().Идентификатор);
	Если Не ЗначениеЗаполнено(СсылкаНаПрофильРедактированиеБанковскихСчетов) Тогда
		Возврат;
	КонецЕсли;

	ПредставлениеПрофиляРедактированиеБанковскихСчетов = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(
		СсылкаНаПрофильРедактированиеБанковскихСчетов, "Наименование");

	РодительДополнительныхГруппДоступа = РодительДополнительныхГруппДоступа();

	ГруппаРедактированиеБанковскихСчетов = Справочники.ГруппыДоступа.СоздатьЭлемент();
	ГруппаРедактированиеБанковскихСчетов.Наименование = ПредставлениеПрофиляРедактированиеБанковскихСчетов;
	ГруппаРедактированиеБанковскихСчетов.Родитель = РодительДополнительныхГруппДоступа;
	ГруппаРедактированиеБанковскихСчетов.Пользователь = Неопределено;
	ГруппаРедактированиеБанковскихСчетов.Профиль = СсылкаНаПрофильРедактированиеБанковскихСчетов;
	ГруппаРедактированиеБанковскихСчетов.Ответственный = Справочники.Пользователи.ПустаяСсылка();
	ГруппаРедактированиеБанковскихСчетов.Комментарий = Справочники.ПрофилиГруппДоступа.ПояснениеПоставляемогоПрофиля(
		СсылкаНаПрофильРедактированиеБанковскихСчетов);

	Для Каждого СтрокаВидыДоступа Из СсылкаНаПрофильРедактированиеБанковскихСчетов.ВидыДоступа Цикл
		ЗаполнитьЗначенияСвойств(ГруппаРедактированиеБанковскихСчетов.ВидыДоступа.Добавить(), СтрокаВидыДоступа);
	КонецЦикла;

	Для Каждого СтрокаЗначенияДоступа Из СсылкаНаПрофильРедактированиеБанковскихСчетов.ВидыДоступа Цикл
		ЗаполнитьЗначенияСвойств(ГруппаРедактированиеБанковскихСчетов.ЗначенияДоступа.Добавить(), СтрокаЗначенияДоступа);
	КонецЦикла;

	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ГруппыДоступаПользователи.Пользователь
	|ИЗ
	|	Справочник.ГруппыДоступа.Пользователи КАК ГруппыДоступаПользователи
	|ГДЕ
	|	ГруппыДоступаПользователи.Ссылка.Профиль.ИдентификаторПоставляемыхДанных В (&ИдентификаторыПрофилей)
	|	И НЕ ГруппыДоступаПользователи.Ссылка.ПометкаУдаления";

	ПрофилиСПравомРедактированияБанковскихСчетов = Новый Массив;
	ПрофилиСПравомРедактированияБанковскихСчетов.Добавить(ПрофильПродажи().УИД);
	ПрофилиСПравомРедактированияБанковскихСчетов.Добавить(ПрофильЗакупки().УИД);

	Запрос.УстановитьПараметр("ИдентификаторыПрофилей", ПрофилиСПравомРедактированияБанковскихСчетов);

	РезультатЗапроса = Запрос.Выполнить();
	Если РезультатЗапроса.Пустой() Тогда
		Возврат;
	КонецЕсли;

	ОшибкиОтсутствуют = Истина;
	Выборка = РезультатЗапроса.Выбрать();

	Пока Выборка.Следующий() Цикл
		НоваяСтрокаПользователь = ГруппаРедактированиеБанковскихСчетов.Пользователи.Добавить();
		НоваяСтрокаПользователь.Пользователь = Выборка.Пользователь;
	КонецЦикла;

	Попытка
		ГруппаРедактированиеБанковскихСчетов.Записать();
	Исключение
		ПодробноеПредставлениеОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
		ОшибкиОтсутствуют = Ложь;
		ЗаписьЖурналаРегистрации(НСтр(
					"ru = 'УправлениеДоступомУНФ.ПредоставитьПользователямДоступКРедактированиюБанковскихСчетов'",
			ОбщегоНазначения.КодОсновногоЯзыка()), УровеньЖурналаРегистрации.Ошибка,
			Метаданные.Справочники.ГруппыДоступа, , ПодробноеПредставлениеОшибки);
	КонецПопытки;
	ПараметрыОбработчика.ОбработкаЗавершена = ОшибкиОтсутствуют;

КонецПроцедуры

#КонецОбласти

