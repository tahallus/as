// Конструктор описания профиля
// 
// Параметры:
// 	Имя - Строка - имя профиля
// 	Идентификатор - Строка - идентификатор
// Возвращаемое значение:
// 	Структура - описание:
// 	* УИД - УникальныйИдентификатор - уникальный идентификатор профиля
// 	* Идентификатор - Строка - строковое представление идентификатора
// 	* Имя - Строка - имя профиля
Функция НовыеСвойстваПрофиля(Имя, Идентификатор) Экспорт

	Результат = Новый Структура;
	Результат.Вставить("Имя", Имя);
	Результат.Вставить("Идентификатор", Идентификатор);
	Результат.Вставить("УИД", Новый УникальныйИдентификатор(Идентификатор));

	Возврат Результат;

КонецФункции

