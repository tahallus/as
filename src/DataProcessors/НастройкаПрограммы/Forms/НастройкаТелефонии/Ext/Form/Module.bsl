	ТекстПодсказкиМаршрутизацияШаблон = "%1
	|
	|%2";
	ТекстПодсказкиМаршрутизация = НСтр("ru='Входящий звонок клиента будет перенаправляться на закрепленного за ним ответственного (указывается в карточке контрагента).'");
	Если ИспользуемаяАТС = ПредопределенноеЗначение("Перечисление.ДоступныеАТС.MangoOffice") Тогда
		ТекстПодсказкиМаршрутизацияАТС = НСтр("ru='В личном кабинете Mango Office в окне настройки входящих линий необходимо добавить ""Звуковое приветствие"" с опцией ""Ожидать окончание - ДА"".'");
	Иначе
		ТекстПодсказкиМаршрутизацияАТС = "";
	КонецЕсли;
	Если ЗначениеЗаполнено(ТекстПодсказкиМаршрутизацияАТС) Тогда
		Элементы.МаршрутизироватьВходящиеНаОтветственных.Подсказка = СтрШаблон(
			ТекстПодсказкиМаршрутизацияШаблон, ТекстПодсказкиМаршрутизация, ТекстПодсказкиМаршрутизацияАТС);
	Иначе
		Элементы.МаршрутизироватьВходящиеНаОтветственных.Подсказка = ТекстПодсказкиМаршрутизация;
	КонецЕсли;
	Элементы.МаршрутизироватьВходящиеНаОтветственных.Видимость = ТелефонияСервер.ИспользоватьНастройкуМаршрутизацииЗвонковНаОтветственного(ИспользуемаяАТС);
	
	Если ЭтоОблачнаяАТС(ИспользуемаяАТС) Тогда
		ПроверитьЗаполнениеНастроекИнтеграции(ИспользуемаяАТС);
		ПроверитьЗаполнениеПользователейТелефонии();
	КонецЕсли;
	
КонецПроцедуры

