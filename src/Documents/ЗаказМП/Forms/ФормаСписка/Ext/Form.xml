	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>ВЫБРАТЬ РАЗЛИЧНЫЕ
	ДокументЗаказ.Ссылка КАК Ссылка,
	ДокументЗаказ.ПометкаУдаления КАК ПометкаУдаления,
	ВЫБОР
		КОГДА ПОДСТРОКА(ДокументЗаказ.Номер, 1, 10) = "0000000000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ДокументЗаказ.Номер, 11, 1)
		КОГДА ПОДСТРОКА(ДокументЗаказ.Номер, 1, 9) = "000000000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ДокументЗаказ.Номер, 10, 2)
		КОГДА ПОДСТРОКА(ДокументЗаказ.Номер, 1, 8) = "00000000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ДокументЗаказ.Номер, 9, 3)
		КОГДА ПОДСТРОКА(ДокументЗаказ.Номер, 1, 7) = "0000000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ДокументЗаказ.Номер, 8, 4)
		КОГДА ПОДСТРОКА(ДокументЗаказ.Номер, 1, 6) = "000000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ДокументЗаказ.Номер, 7, 5)
		КОГДА ПОДСТРОКА(ДокументЗаказ.Номер, 1, 5) = "00000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ДокументЗаказ.Номер, 6, 6)
		КОГДА ПОДСТРОКА(ДокументЗаказ.Номер, 1, 4) = "0000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ДокументЗаказ.Номер, 5, 7)
		КОГДА ПОДСТРОКА(ДокументЗаказ.Номер, 1, 3) = "000"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ДокументЗаказ.Номер, 4, 8)
		КОГДА ПОДСТРОКА(ДокументЗаказ.Номер, 1, 2) = "00"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ДокументЗаказ.Номер, 3, 9)
		КОГДА ПОДСТРОКА(ДокументЗаказ.Номер, 1, 1) = "0"
			ТОГДА "" + &amp;СимволНомера + ПОДСТРОКА(ДокументЗаказ.Номер, 2, 10)
		ИНАЧЕ ДокументЗаказ.Номер
	КОНЕЦ КАК Номер,
	ВЫБОР
		КОГДА СинхронизацияВключенаМП.Значение
				И ЗаказМПИзменения.Узел &lt;&gt; НЕОПРЕДЕЛЕНО
			ТОГДА 6
		КОГДА ДокументЗаказ.ПометкаУдаления
			ТОГДА 5
		КОГДА НЕ ДокументЗаказ.Проведен
			ТОГДА 0
		КОГДА ДокументЗаказ.СостояниеЗаказа = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказовМП.Выполнен)
			ТОГДА 3
		КОГДА ДокументЗаказ.Оплачен
			ТОГДА 1
		КОГДА ДокументЗаказ.Отгружен
			ТОГДА 2
		КОГДА ДокументЗаказ.СостояниеЗаказа = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказовМП.ВРаботе)
			ТОГДА 4
	КОНЕЦ КАК ИндексКартинки,
	ДокументЗаказ.Дата КАК Дата,
	ДокументЗаказ.Проведен КАК Проведен,
	ДокументЗаказ.Покупатель КАК Покупатель,
	ДокументЗаказ.СостояниеЗаказа КАК СостояниеЗаказа,
	ДокументЗаказ.Отгружен КАК Отгружен,
	ДокументЗаказ.Оплачен КАК Оплачен,
	ДокументЗаказ.СуммаДокумента КАК СуммаДокумента,
	ДокументЗаказ.ИзЦентральнойБазы КАК ИзЦентральнойБазы,
	ДокументЗаказ.ОтгруженУстановленВЦентральнойБазе КАК ОтгруженУстановленВЦентральнойБазе,
	ДокументЗаказ.ОплаченУстановленВЦентральнойБазе КАК ОплаченУстановленВЦентральнойБазе,
	ДокументЗаказ.СуммаОплаты КАК СуммаОплаты,
	ДокументЗаказ.МоментВремени КАК МоментВремени,
	ДокументЗаказ.НомерПодтвержден КАК НомерПодтвержден,
	СинхронизацияВключенаМП.Значение КАК СинхронизацияВключенаМП,
	ЗаказМПИзменения.Узел КАК Узел
ИЗ
	Константа.СинхронизацияВключенаМП КАК СинхронизацияВключенаМП,
	Документ.ЗаказМП КАК ДокументЗаказ
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ЗаказМП.Изменения КАК ЗаказМПИзменения
		ПО (ЗаказМПИзменения.Ссылка = ДокументЗаказ.Ссылка)</QueryText>
