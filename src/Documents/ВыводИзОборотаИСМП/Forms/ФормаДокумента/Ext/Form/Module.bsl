		Элементы.Причина.СписокВыбора.Добавить(Операция.Значение, Операция.Представление);
	КонецЦикла;
	
	Элементы.НаименованиеПервичногоДокумента.Видимость = (Объект.ВидПервичногоДокумента = ПредопределенноеЗначение("Перечисление.ВидыПервичныхДокументовИСМП.Прочее"));
	
	Если ИнтеграцияИСКлиентСервер.ЭтоПродукцияМОТП(Объект.ВидПродукции) Тогда
		Элементы.АдресПлощадкиСтрокой.Видимость               = Истина;
		Элементы.ДатаПервичногоДокумента.ФорматРедактирования = "";
	Иначе
		Элементы.АдресПлощадкиСтрокой.Видимость               = Ложь;
		Элементы.ДатаПервичногоДокумента.ФорматРедактирования = "ДФ=dd.MM.yyyy;";
	КонецЕсли;
	
КонецПроцедуры

