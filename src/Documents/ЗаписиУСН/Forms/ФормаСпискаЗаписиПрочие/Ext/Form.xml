					<v8:content>КУДиР</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>КУДиР.Recorder</Field>
				<Field>КУДиР.ВидЗаписи</Field>
				<Field>КУДиР.ГодДокумента</Field>
