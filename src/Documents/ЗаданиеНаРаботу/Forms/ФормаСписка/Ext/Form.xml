		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>Список.Ref</Field>
				<Field>Список.ВидОперации</Field>
				<Field>Список.ВидРабот</Field>
				<Field>Список.Организация</Field>
				<Field>Список.Состояние</Field>
				<Field>Список.Сотрудник</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ДокументЗаданиеНаРаботу.Ссылка,
	ДокументЗаданиеНаРаботу.ВерсияДанных,
	ДокументЗаданиеНаРаботу.ПометкаУдаления,
	ДокументЗаданиеНаРаботу.Номер,
	ДокументЗаданиеНаРаботу.Дата,
	ДокументЗаданиеНаРаботу.Проведен,
	ДокументЗаданиеНаРаботу.Организация,
	ДокументЗаданиеНаРаботу.ВидОперации,
	ДокументЗаданиеНаРаботу.Состояние,
	ДокументЗаданиеНаРаботу.ВидРабот,
	ДокументЗаданиеНаРаботу.ВидЦен,
	ДокументЗаданиеНаРаботу.Сотрудник,
	ДокументЗаданиеНаРаботу.СтруктурнаяЕдиница,
	ДокументЗаданиеНаРаботу.СуммаДокумента,
	ДокументЗаданиеНаРаботу.ПоложениеВидаРабот,
	ДокументЗаданиеНаРаботу.Событие,
	ДокументЗаданиеНаРаботу.Комментарий,
	ДокументЗаданиеНаРаботу.Автор,
	ДокументЗаданиеНаРаботу.Работы.(
		Ссылка,
		НомерСтроки,
		ВидРабот,
		Заказчик,
		Номенклатура,
		Характеристика,
		ДатаНачала,
		ДатаОкончания,
		Трудоемкость,
		Цена,
		Сумма,
		Комментарий
	),
	ДокументЗаданиеНаРаботу.МоментВремени,
	ВЫБОР
		КОГДА &amp;ПериодНачала = ДАТАВРЕМЯ(1, 1, 1)
				И 1 В
					(ВЫБРАТЬ ПЕРВЫЕ 1
						1
					ИЗ
						РегистрНакопления.ЗаданияНаРаботу КАК ТаблицаПросроченные
					ГДЕ
						ТаблицаПросроченные.Регистратор = ДокументЗаданиеНаРаботу.Ссылка
						И ТаблицаПросроченные.ВремяОкончания &lt; &amp;ПериодОкончания
						И ТаблицаПросроченные.ВремяНачала &lt;&gt; ДАТАВРЕМЯ(1, 1, 1))
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК Просрочено,
	ВЫБОР
		КОГДА &amp;ПериодНачала &lt;&gt; ДАТАВРЕМЯ(1, 1, 1)
				И 1 В
					(ВЫБРАТЬ ПЕРВЫЕ 1
						1
					ИЗ
						РегистрНакопления.ЗаданияНаРаботу КАК ТаблицаНаСегодня
					ГДЕ
						ТаблицаНаСегодня.Регистратор = ДокументЗаданиеНаРаботу.Ссылка
						И ТаблицаНаСегодня.ВремяОкончания &gt;= &amp;ПериодОкончания
						И ТаблицаНаСегодня.ВремяНачала &lt;= &amp;ПериодНачала)
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК НаСегодня,
	ВЫБОР
		КОГДА НаличиеФайлов.ЕстьФайлы ЕСТЬ NULL
			ТОГДА 0
		КОГДА НаличиеФайлов.ЕстьФайлы
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК ЕстьФайлы
ИЗ
	Документ.ЗаданиеНаРаботу КАК ДокументЗаданиеНаРаботу
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.НаличиеФайлов КАК НаличиеФайлов
		ПО ДокументЗаданиеНаРаботу.Ссылка = НаличиеФайлов.ОбъектСФайлами}</QueryText>
				<MainTable>Document.ЗаданиеНаРаботу</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
