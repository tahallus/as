	ИначеЕсли НЕ СтраницаИнформацияЗаполнена Тогда
		SVG = SVG + Символы.ПС + "<image xlink:href=""data:image/png;base64,"" id=""svg_6"" height=""26"" width=""247"" y=""100.98304"" x=""11.740113""/>
		|<text xml:space=""preserve"" text-anchor=""start"" font-family=""Helvetica, Arial, sans-serif"" font-size=""9"" id=""svg_5"" y=""117"" x=""115.466105"" stroke-width=""0"" stroke=""#000"" fill=""#009646"">Контакты</text>";
		
