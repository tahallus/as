&НаКлиенте
Процедура ЗаполнитьВкладкуОНасАвтоматически(Команда)
	
	ТекстHTML = "";
	Если ЗначениеЗаполнено(Объект.Наименование) Тогда
		ТекстHTML = "<p style=""text-align: center;""><strong>" + Объект.Наименование +  "</strong> </p>";
	КонецЕсли;
	
	Если ЗначениеЗаполнено(Объект.РасширениеЛоготипа) Тогда
		ТекстHTML = ТекстHTML + ПолучитьЛоготип();
	КонецЕсли;
	
	Если ЗначениеЗаполнено(Объект.ИнформацияОКомпании) Тогда
		ТекстHTML = ТекстHTML + Символы.ПС + "<p style=""text-align: center;"">" + Объект.ИнформацияОКомпании + "</p>";
	КонецЕсли;
	
	ЗаполнитьВкладкуОНасАвтоматическиФрагмент(ТекстHTML);
	
	УстановитьПредпросмотрВкладкиОНас("Информация");
	
	ВерсияОНасИзменяемая = ВерсияОНасИзменяемая + 1;
	
	УстановитьВидимостьПубликацииЕслиНужно(Истина);
	
КонецПроцедуры

&НаКлиенте
Функция ПолучитьЛоготип()
	
	Если ЗначениеЗаполнено(Объект.РасширениеЛоготипа) Тогда
		
		Логотип = ПолучитьЛоготипФрагмент();
		
		Формат = Объект.РасширениеЛоготипа;
		
		Если Логотип <> Неопределено Тогда
			
			ПодготовленнаяСтрока = СтрЗаменить(Base64Строка(Логотип), Символы.ПС, "");
			ПодготовленнаяСтрока = СтрЗаменить(ПодготовленнаяСтрока, Символы.ВК, "");
			СтруктураСРазмерами = ВычислитьВысотуИШиринуЛоготипа();
			КартинкаВСтроке = "<p style=""text-align: center;"">" + Символы.ПС + "<img width=" + СтруктураСРазмерами.Ширина + " height=" + СтруктураСРазмерами.Высота + " src=""" + "data:image/" + Формат + ";base64," + ПодготовленнаяСтрока + """></p>";
			
			Возврат КартинкаВСтроке;
			
		Иначе
			
			Возврат "";
			
		КонецЕсли;
	Иначе
		
		Возврат "";
		
	КонецЕсли;
	
КонецФункции

&НаСервере
Функция ПолучитьЛоготипФрагмент()
	
	Перем ВыборкаДетальныеЗаписи, Запрос, РезультатЗапроса;
	
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ ПЕРВЫЕ 1
	|	НастройкиПубликацииМЛК.Логотип КАК Логотип
	|ИЗ
	|	Справочник.НастройкиПубликацииМЛК КАК НастройкиПубликацииМЛК";
	
	РезультатЗапроса = Запрос.Выполнить();
	
	ВыборкаДетальныеЗаписи = РезультатЗапроса.Выбрать();
	
	Если ВыборкаДетальныеЗаписи.Следующий() Тогда
		Возврат ВыборкаДетальныеЗаписи.Логотип.Получить();
	КонецЕсли;
	
КонецФункции

&НаКлиенте
Функция ВычислитьВысотуИШиринуЛоготипа()
	
	Ширина = ШиринаЛоготипа;
	Высота = ВысотаЛоготипа;
	
	Если Ширина <= 100 И Высота <= 100 Тогда
		Возврат Новый Структура("Ширина, Высота", Ширина, Высота)
	ИначеЕсли Ширина < Высота Тогда
		Делимое = Высота;
	ИначеЕсли Ширина = Высота Тогда
		Делимое = Ширина;
	ИначеЕсли Ширина > Высота Тогда
		Делимое = Ширина;
	КонецЕсли;
	
	Разница = Цел(Делимое/100);
	
	Ширина = Цел(Ширина/Разница);
	Высота = Цел(Высота/Разница);
	
	Возврат Новый Структура("Ширина, Высота", Строка(Ширина), Строка(Высота))
	
КонецФункции

