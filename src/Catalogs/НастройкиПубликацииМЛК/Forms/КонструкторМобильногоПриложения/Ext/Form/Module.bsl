
#Область ОписаниеПеременных

&НаКлиенте
Перем ДанныеВыбораСостояния;

&НаКлиенте
Перем ДанныеВыбораСостоянияДляНовыхКлиентов;

#КонецОбласти

#Область ОбработчикиСобытийФормы

&НаСервере
Процедура ПриСозданииНаСервере(Отказ, СтандартнаяОбработка)
	
	Если МобильныйКлиентУНФ.ФункциональностьВременноНедоступна(Отказ) Тогда
		Возврат;
	КонецЕсли;
	
	РазделениеВключено = ОбщегоНазначения.РазделениеВключено();
	ЭтоСервис1СФреш = Справочники.НастройкиПубликацииМЛК.ЭтоСервис1СФреш();
	
	ТекПользователь = Пользователи.ТекущийПользователь();
	
	ОсновнаяОрганизация = УправлениеНебольшойФирмойПовтИсп.ПолучитьЗначениеПоУмолчаниюПользователя(ТекПользователь, "ОсновнаяОрганизация");
	
	Если НЕ ЗначениеЗаполнено(ОсновнаяОрганизация) Тогда
		ОсновнаяОрганизация = Справочники.Организации.ОрганизацияПоУмолчанию();
	КонецЕсли;
	
	ПочтаОрганизации = ПечатьДокументовУНФ.КонтактнаяИнформация(ОсновнаяОрганизация, Справочники.ВидыКонтактнойИнформации.EmailОрганизации); 
	ФактАдресОрганизации = ПечатьДокументовУНФ.КонтактнаяИнформация(ОсновнаяОрганизация, Справочники.ВидыКонтактнойИнформации.ФактАдресОрганизации);
	ТелефонОрганизации = ПечатьДокументовУНФ.КонтактнаяИнформация(ОсновнаяОрганизация, Справочники.ВидыКонтактнойИнформации.ТелефонОрганизации);
	СайтОрганизации = ПечатьДокументовУНФ.КонтактнаяИнформация(ОсновнаяОрганизация, Справочники.ВидыКонтактнойИнформации.СайтОрганизации);
	
	ИспользоватьПодсистемуРаботы = ПолучитьФункциональнуюОпцию("ИспользоватьПодсистемуРаботы");
	
	Если НЕ ЗначениеЗаполнено(Объект.Ссылка) Тогда
		
		Объект.Почта = ПочтаОрганизации;
		Объект.Адрес = ФактАдресОрганизации;
		Объект.Телефон = ТелефонОрганизации;
		Объект.Сайт = СайтОрганизации;
		Объект.Наименование = ОсновнаяОрганизация.Наименование;
		Объект.МинимальныйИнтервалУслуги = 15;
		Объект.РежимПодтвержденияЗаписи = Перечисления.РежимыПодтвержденияЗаписи.НеТребуется;
		Объект.НаименованиеРесурса = "Сотрудник";
		Объект.НаименованиеРесурсаВРодительномПадеже = "сотрудника";
		Объект.НаименованиеРесурсаВДательномПадеже = "сотруднику";
		Объект.НаименованиеРесурсаВВинительномПадеже = "сотрудника";
		Объект.ВариантЗаписиНаУслуги = Перечисления.ВариантыЗаписиНаУслуги.ЗаписьНаВремя;
		Объект.ВариантОтбораУслуг = Перечисления.ВариантыОтбораУслуг.НеИспользовать;
		
		Объект.ВидЗаказНаряда = Справочники.ВидыЗаказНарядов.Основной;
		ВидЗаказаПриИзмененииСервер();
		
		Объект.ВидЗаказНарядаДляНовыхКлиентов = Справочники.ВидыЗаказНарядов.Основной;
		ВидЗаказНарядаДляНовыхКлиентовПриИзмененииСервер();
		
		Объект.ВариантПодтвержденияНовогоКлиента = Перечисления.ВариантыПодтвержденияНовогоКлиента.Вручную;
		
		Объект.ВидЗаказаПокупателя = Справочники.ВидыЗаказовПокупателей.Основной;
		ВидВидЗаказПокупателяПриИзмененииСервер();
		
		Объект.ИспользоватьОНас = Истина;
		Объект.ИспользоватьЗаказТоваров = Истина;
		Объект.ИспользоватьКартинкиТоваров = Истина;
		
		Если ЗначениеЗаполнено(ОсновнаяОрганизация.ФайлЛоготип) Тогда
			ДвоичныеДанныеКартинки = РаботаСФайламиУНФ.СсылкаНаДвоичныеДанныеФайла(ОсновнаяОрганизация.ФайлЛоготип, УникальныйИдентификатор);
			Если ДвоичныеДанныеКартинки <> Неопределено Тогда
				ЗаполнитьКартинкуНаСервере(ДвоичныеДанныеКартинки);
			КонецЕсли;
		КонецЕсли;
		
		Объект.ВидЦенТоваров = Справочники.ВИдыЦен.ПолучитьОсновнойВидЦенПродажи();
		Объект.ВидЦенУслуг = Справочники.ВИдыЦен.ПолучитьОсновнойВидЦенПродажи();
		Объект.ОтборГруппыКатегорииНоменклатуры = Перечисления.ВидыОтборовНоменклатуры.КатегорииНоменклатуры;
		Объект.ИспользоватьДоставкуКурьером = Истина;
		
		Объект.ДатаСоздания = ТекущаяДатаСеанса();
		
		Объект.ВариантОтображенияКартинок = Перечисления.ВариантыОтображенияКартинок.ТолькоОсновная;
		
	Иначе
		
		ДобавитьПунктыВыдачиЗаказовНаФорму();
		
		Если Не ЗначениеЗаполнено(Объект.ВидЦенТоваров) Тогда
			Объект.ВидЦенТоваров = Справочники.ВИдыЦен.ПолучитьОсновнойВидЦенПродажи();
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(Объект.ВидЦенУслуг) Тогда
			Объект.ВидЦенУслуг = Справочники.ВИдыЦен.ПолучитьОсновнойВидЦенПродажи();
		КонецЕсли;

		Если Не ЗначениеЗаполнено(Объект.ОтборГруппыКатегорииНоменклатуры) Тогда
			Объект.ОтборГруппыКатегорииНоменклатуры = Перечисления.ВидыОтборовНоменклатуры.ГруппыНоменклатуры;
			Объект.ИспользоватьОНас = Истина;
			Объект.ИспользоватьЗаказТоваров = Истина;
			Объект.ИспользоватьКартинкиТоваров = Истина;
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(Объект.СостояниеЗаказаПокупателя) Тогда
			Объект.ВидЗаказаПокупателя = Справочники.ВидыЗаказовПокупателей.Основной;
			ВидВидЗаказПокупателяПриИзмененииСервер();
		КонецЕсли;
		
		Если Не Объект.ИспользоватьДоставкуКурьером И Не Объект.ИспользоватьДоставкуПоПочте И Не Объект.ИспользоватьДоставкуВПунктыВыдачиЗаказа Тогда
			Объект.ИспользоватьДоставкуКурьером = Истина;
		КонецЕсли;
		
		// Новостной центр
		УстановитьБаннерыНаФорме();
		УстановитьПодсказкиАссистентаВКонструкторе();
		
	КонецЕсли;
	
	УстановитьВысотуИШиринуЛоготипа();
	
	Элементы.Почта.ПодсказкаВвода = ПочтаОрганизации;
	Элементы.Адрес.ПодсказкаВвода = ФактАдресОрганизации;
	Элементы.Телефон.ПодсказкаВвода = ТелефонОрганизации;
	Элементы.Сайт.ПодсказкаВвода = СайтОрганизации;
	Элементы.НаименованиеМобильногоПриложения.ПодсказкаВвода = ОсновнаяОрганизация.Наименование;
	
	Если Объект.РежимПодтвержденияЗаписи = Перечисления.РежимыПодтвержденияЗаписи.ПоSMS 
		ИЛИ Объект.РежимПодтвержденияЗаписи = Перечисления.РежимыПодтвержденияЗаписи.Гибридный Тогда
		Элементы.НастройкаОтправкиSMS.Видимость = Истина;
	Иначе
		Элементы.НастройкаОтправкиSMS.Видимость = Ложь;
	КонецЕсли;
	
	ЗаполнитьСтраницуОНас();
	
	УстановитьКодыУбратьВидимостьЛогинаПароля();
	Если ТекущиеНастройкиКабинетКлиента <> Неопределено Тогда
		ПользовательСервисаЛогин = ТекущиеНастройкиКабинетКлиента.EmailПриРегистрации;
	КонецЕсли;
	
	УстановитьЛоготипНаФорме();
	
	УстановитьСценарийЗаписи();
	
	НастроитьВидИСостояниеНаФорме();
	
	НастройкиВыгрузкиРеквизитовПредставление = НастройкиВыгрузкиРеквизитовПредставление(Объект.НастройкиВыгрузкиРеквизитов);
	ПолучитьАдресКаталогаТоваров();
	Элементы.НастроитьКаталог.Заголовок = ПредставлениеКомандыНастроитьКаталогТоваров(АдресКаталогаТоваров);
	
	УстановитьВидимостьЭлементовПодменю();
	УстановитьВидимостьЭлементовЗакладкиТовары();
	
	Если Объект.СтоимостьДоставкиКурьером > 0 Тогда
		Элементы.ГруппаБесплатнаяДоставкаОт.Видимость = Истина;
	Иначе
		Элементы.ГруппаБесплатнаяДоставкаОт.Видимость = Ложь;
	КонецЕсли;
	
	Если НЕ ЗначениеЗаполнено(Объект.Организация) Тогда
		Объект.Организация = ОсновнаяОрганизация;
	КонецЕсли;
	
	Если НЕ ЗначениеЗаполнено(Объект.ОрганизацияДляОстатков) Тогда
		Объект.ОрганизацияДляОстатков = ОсновнаяОрганизация;
	КонецЕсли;
	
	Объект.ПубликацияВыполняется = Ложь;
	
	СкрытьРазвернутьПодменю(Ложь);
	
	ЗаполнитьСписокФайлов();
	
	ПоказатьНеобходимостьПовторнойПубликации(Объект.НужноОпубликовать);
	
	ЗаполнитьСписокСкладов();
	СформироватьЭлементыФормыСкладов();
	СформироватьЗаголовокГруппыСкладов();
	
	Элементы.ГруппаСклады.Видимость = ПолучитьФункциональнуюОпцию("УчетПоНесколькимСкладам");
	Элементы.ГруппаОрганизацияСклады.Видимость = ПолучитьФункциональнуюОпцию("ИспользоватьНесколькоОрганизаций");
	
	УстановитьПараметрыФото();
	
	// Новостной центр
	Элементы.ГруппаНастройкаПодробнойНовости.Видимость = Ложь;
	Элементы.УведомлениеДатаОтправки.Видимость = Ложь;
	ДатаИВремяОтправкиУведомления = ТекущаяДатаСеанса() + 300;
	
	ПолучитьАдресИндивидуальныхЦен();
	
КонецПроцедуры

&НаКлиенте
Процедура ПриОткрытии(Отказ)
	
	ОтобразитьСтраницу("Главное");
	
	Если ЗначениеЗаполнено(Объект.Ссылка) Тогда
		ВерсияОнлайнЗаписиИзменяемая = 0;
		ВерсияОНасИзменяемая = 0;
		ВерсияОсновныхДанныхИзменяемая = 0;
		ВерсияЛоготипаИзменяемая = 0;
		ВерсияЗаказТоваровИзменяемая = 0;
	Иначе
		ВерсияОнлайнЗаписиИзменяемая = 1;
		ВерсияОНасИзменяемая = 1;
		ВерсияОсновныхДанныхИзменяемая = 1;
		ВерсияЛоготипаИзменяемая = 1;
		ВерсияЗаказТоваровИзменяемая = 1;
	КонецЕсли;
	
	Если Объект.ИспользоватьОНас Тогда
		ТекущаяСтраницаВкладки = "Страница_ОНас";
	ИначеЕсли Объект.ИспользоватьОнлайнЗапись Тогда
		ТекущаяСтраницаВкладки = "Страница_ОнлайнЗапись";
	КонецЕсли;
	
	УстановитьВидимостьОнлайнЗаписи();
	УстановитьВидимостьОНас();
	
	УстановитьПредпросмотрСтраницыПубликация();
	
	ИзменитьВариантЗаписиНаУслугиНаФорме();
	НаличиеРесурсаПриИзмененииФрагмент();
	
	//УстановитьУправлениеЗаписями();
	
	Если Объект.ВариантОтбораУслуг = ПредопределенноеЗначение("Перечисление.ВариантыОтбораУслуг.НеИспользовать") ИЛИ Не ЗначениеЗаполнено(Объект.ВариантОтбораУслуг) Тогда
		ИспользоватьОтборУслуг = Ложь;
	Иначе
		ИспользоватьОтборУслуг = Истина;
	КонецЕсли;
	
	УстановитьВидимостьВариантаОтбораУслуг();
	
	//ОпределитьВидимостьВариантПодтвержденияНовогоКлиента();
	
	УправлениеДоступностьюПубликации();
	СформироватьЗаголовокИнструкции();
	УстановитьСтраницуАвторизацииВыполнениеПубликации();
	Если Объект.ПубликацияВыполняется Тогда
		ВыбратьПунктМенюНаСервере("Публикация");
	Иначе
		ВыбратьПунктМенюНаСервере("Главное");
	КонецЕсли;
	
	УстановитьВидимостьВариантаОтображенияКартинок();
	
КонецПроцедуры

&НаСервере
Процедура ПередЗаписьюНаСервере(Отказ, ТекущийОбъект, ПараметрыЗаписи)
	
	ТекущийОбъект.ПоследнийПользователь = Пользователи.ТекущийПользователь();
	
	Если Объект.ВариантЗаписиНаУслуги = Перечисления.ВариантыЗаписиНаУслуги.ЗаписьБезВремени Тогда
		
		Для каждого Строка Из НастройкиЗаписьНаУслугиБезРесурса Цикл
			
			Если Строка.Услуга = Справочники.Номенклатура.ПустаяСсылка() Тогда
				Продолжить;
			КонецЕсли;
			
			НЗ = РегистрыСведений.НастройкиЗаписьНаУслуги.СоздатьНаборЗаписей();
			НЗ.Отбор.ИсточникЗаписи.Установить(Перечисления.ЗаписьНаУслугиИсточник.МобильноеПриложение);
			НЗ.Отбор.Услуга.Установить(Строка.Услуга);
			НЗ.Отбор.Ресурс.Установить(Справочники.КлючевыеРесурсы.ПустаяСсылка());
			
			НовСтр = НЗ.Добавить();
			НовСтр.Услуга = Строка.Услуга;
			НовСтр.Длительность = Строка.Длительность;
			НовСтр.ИсточникЗаписи = Перечисления.ЗаписьНаУслугиИсточник.МобильноеПриложение;
			НЗ.Записать(Истина);
			
		КонецЦикла;
		
	КонецЕсли;
	
	// Создаем готовые пакеты для мобильного приложения
	
	// ОНЛАЙН ЗАПИСЬ
	Если ВерсияОнлайнЗаписиИзменяемая > 0 Тогда
		
		ТекущийОбъект.ВерсияОнлайнЗаписи = ТекущийОбъект.ВерсияОнлайнЗаписи + 1;
		
		КонструкторМобильногоПриложения.СформироватьПакетСУслугами(ТекущийОбъект, Объект.ВидЦенУслуг);
		КонструкторМобильногоПриложения.СформироватьПакетСРесурсами(ТекущийОбъект);
		КонструкторМобильногоПриложения.СформироватьПакетСРесурсамиИУслугами(ТекущийОбъект);
		
		СформироватьСценарийЗаписи(ТекущийОбъект);
		
	КонецЕсли;
	
	// О НАС
	Если ВерсияОНасИзменяемая > 0  Тогда
		
		ТекущийОбъект.ВерсияОНас = ТекущийОбъект.ВерсияОНас + 1;
		
		Если ТекущийОбъект.ИспользоватьОНас Тогда
			
			СтрокаHTML = "";
			Вложение = Новый Структура;
			
			СодержимоеСтраницы.ПолучитьHTML(СтрокаHTML, Вложение);
			
			ТекущийОбъект.ОНасСтрокаHTML = Новый ХранилищеЗначения(СтрокаHTML, Новый СжатиеДанных(9));
			ТекущийОбъект.ОНасВложение = Новый ХранилищеЗначения(Вложение, Новый СжатиеДанных(9));
			
			Если Вложение.Количество() > 0 Тогда
				ТекущийОбъект.HTMLДляМЛК = ПреобразоватьHTML(СтрокаHTML, Вложение);
			Иначе
				ТекущийОбъект.HTMLДляМЛК = СтрокаHTML;
			КонецЕсли;
			
			СоциальныеСетиСтруктура = Новый Структура;
			СоциальныеСетиСтруктура.Вставить("Инстаграм", СокрЛП(ТекущийОбъект.СсылкаНаИнстаграм));
			СоциальныеСетиСтруктура.Вставить("Фейсбук", СокрЛП(ТекущийОбъект.СсылкаНаФейсбук));
			СоциальныеСетиСтруктура.Вставить("ВК", СокрЛП(ТекущийОбъект.СсылкаНаВКонтакте));
			СоциальныеСетиСтруктура.Вставить("Ютуб", СокрЛП(ТекущийОбъект.СсылкаНаЮтуб));
			
			СоциальныеСетиХранилищеЗначения = Новый ХранилищеЗначения(СоциальныеСетиСтруктура, Новый СжатиеДанных(9));
			
			СтруктураПакетОНас = Новый Структура;
			СтруктураПакетОНас.Вставить("HTMLДляМЛК", Новый ХранилищеЗначения(ТекущийОбъект.HTMLДляМЛК, Новый СжатиеДанных(9)));
			СтруктураПакетОНас.Вставить("Сайт", ТекущийОбъект.Сайт);
			СтруктураПакетОНас.Вставить("Почта", ТекущийОбъект.Почта);
			СтруктураПакетОНас.Вставить("Телефон", ТекущийОбъект.Телефон);
			СтруктураПакетОНас.Вставить("Адрес", ТекущийОбъект.Адрес);
			СтруктураПакетОНас.Вставить("СоциальныеСети", Новый ХранилищеЗначения(СоциальныеСетиСтруктура, Новый СжатиеДанных(9)));
			
			ТекущийОбъект.ПакетОНас = Новый ХранилищеЗначения(СтруктураПакетОНас, Новый СжатиеДанных(9));
			
		КонецЕсли;
		
	КонецЕсли;
	
	// ЛОГОТИП
	Если ВерсияЛоготипаИзменяемая > 0 Тогда
		ТекущийОбъект.ВерсияЛоготипа = ТекущийОбъект.ВерсияЛоготипа + 1;
	КонецЕсли;
	
	// ОСНОВНЫЕ ДАННЫЕ
	Если ВерсияОсновныхДанныхИзменяемая > 0 Тогда
		
		ТекущийОбъект.ВерсияОсновныхДанных = ТекущийОбъект.ВерсияОсновныхДанных + 1;
		
		СтруктураСОсновнымиДанными = Новый Структура("НаименованиеМобильногоПриложения", ТекущийОбъект.Наименование);
		ТекущийОбъект.ПакетСОсновнымиДанными = Новый ХранилищеЗначения(СтруктураСОсновнымиДанными, Новый СжатиеДанных(9));
		
	КонецЕсли;
	
	//Если ВерсияЗаказТоваровИзменяемая > 0 Тогда
	//	СформироватьПакетЗаказТоваров(ТекущийОбъект);
	//КонецЕсли;
	
	Если АдресКаталогаТоваров <> "" Тогда
		Каталог = ПолучитьИзВременногоХранилища(АдресКаталогаТоваров);
		ТекущийОбъект.КаталогТоваров = Новый ХранилищеЗначения(Каталог, Новый СжатиеДанных(9));
	КонецЕсли;
	
	Если АдресИндивидуальныхЦен <> "" Тогда
		ИндивидуальныеЦены = ПолучитьИзВременногоХранилища(АдресИндивидуальныхЦен);
		ТекущийОбъект.ИндивидуальныеЦены.Загрузить(ИндивидуальныеЦены);
	КонецЕсли;
	
	//СтрокаHTML = "";
	//Вложение = Новый Структура;
	//
	//СодержимоеСтраницы.ПолучитьHTML(СтрокаHTML, Вложение);
	//
	//Объект.HTMLДляМЛК = ПреобразоватьHTML(СтрокаHTML, Вложение);
	
	Если ВерсияЗаказТоваровИзменяемая > 0 Тогда
		
		ПунктыВыдачиЗаказа = Новый ТаблицаЗначений;
		ПунктыВыдачиЗаказа.Колонки.Добавить("Наименование");
		ПунктыВыдачиЗаказа.Колонки.Добавить("УИПВЗ");
		ПунктыВыдачиЗаказа.Колонки.Добавить("Адрес");
		ПунктыВыдачиЗаказа.Колонки.Добавить("Стоимость");
		
		Если ЗначениеЗаполнено(ПунктВыдачиЗаказа) Тогда
			СтрПВЗ = ПунктыВыдачиЗаказа.Добавить();
			СтрПВЗ.Наименование = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(ПунктВыдачиЗаказа, "Наименование");
			СтрПВЗ.УИПВЗ = ПунктВыдачиЗаказа.УникальныйИдентификатор();
			СтрПВЗ.Стоимость = СтоимостьДоставкиВПунктВыдачи;
			СтрПВЗ.Адрес = АдресПунктаВыдачиЗаказа;
		КонецЕсли;
		
		ТекущийОбъект.ПунктыВыдачиЗаказа = Новый ХранилищеЗначения(ПунктыВыдачиЗаказа, Новый СжатиеДанных(9));
		
	КонецЕсли;
	
	
	
КонецПроцедуры

&НаСервере
Процедура ОбработкаПроверкиЗаполненияНаСервере(Отказ, ПроверяемыеРеквизиты)
	
	Если Объект.ИспользоватьОнлайнЗапись Тогда
		
		Если Объект.ВариантЗаписиНаУслуги = Перечисления.ВариантыЗаписиНаУслуги.ЗаписьНаВремя И Не Объект.ИспользоватьВыборРесурса И Не ЗначениеЗаполнено(Объект.РежимЗаполненияРесурса) Тогда
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Поле ""Режим заполнение ресурса"" не заполнено'"),
			,
			"Объект.РежимЗаполненияРесурса");
			Отказ = Истина;
		КонецЕсли;
		
		Если ИспользоватьВидыЗаказНарядов И Объект.ТребоватьПодтвержденияДляНовогоКлиента И Объект.ВариантПодтвержденияНовогоКлиента = Перечисления.ВариантыПодтвержденияНовогоКлиента.Вручную И Не ЗначениеЗаполнено(Объект.ВидЗаказНарядаДляНовыхКлиентов) Тогда
			
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Поле ""Вид заказ-наряда"" не заполнено'"),
			,
			"Объект.ВидЗаказНарядаДляНовыхКлиентов");
			Отказ = Истина;
			
		КонецЕсли;
		
		Если Объект.ТребоватьПодтвержденияДляНовогоКлиента И Объект.ВариантПодтвержденияНовогоКлиента = Перечисления.ВариантыПодтвержденияНовогоКлиента.Вручную И Не ЗначениеЗаполнено(Объект.СостояниеЗаказНарядаДляНовыхКлиентов) Тогда
			
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Поле ""Состояние заказ-наряда"" не заполнено'"),
			,
			"Объект.СостояниеЗаказНарядаДляНовыхКлиентов");
			Отказ = Истина;
			
		КонецЕсли;
		
		Если ИспользоватьВидыЗаказНарядов И Не ЗначениеЗаполнено(Объект.ВидЗаказНаряда) Тогда
			
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Поле ""Вид заказ-наряда"" не заполнено'"),
			,
			"Объект.ВидЗаказНаряда");
			Отказ = Истина;
			
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(Объект.СостояниеЗаказНаряда) Тогда
			
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Поле ""Состояние заказ-наряда"" не заполнено'"),
			,
			"Объект.СостояниеЗаказНаряда");
			Отказ = Истина;
			
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(Объект.НаименованиеРесурса) Тогда
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Поле ""Наименование ресурса"" не заполнено'"),
			,
			"Объект.НаименованиеРесурса");
			Отказ = Истина;
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(Объект.МинимальныйИнтервалУслуги) Тогда
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Поле ""Интервал записи"" не заполнено'"),
			,
			"Объект.МинимальныйИнтервалУслуги");
			Отказ = Истина;
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(Объект.ВидЦенУслуг) Тогда
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Поле ""Вид цен услуг"" не заполнено'"),
			,
			"Объект.ВидЦенУслуг");
			Отказ = Истина;
		КонецЕсли;
		
		Если Объект.ВариантПодтвержденияНовогоКлиента = Перечисления.ВариантыПодтвержденияНовогоКлиента.ПоSMS Тогда
			
			УстановитьПривилегированныйРежим(Истина);
			НастройкиОтправкиSMS = НастройкиОтправкиSMS();
			УстановитьПривилегированныйРежим(Ложь);
			
			ЛогинДляОтправкиSMS = НастройкиОтправкиSMS.Логин;
			ПарольДляОтправкиSMS = НастройкиОтправкиSMS.Пароль;
			
			Если Не ЗначениеЗаполнено(ЛогинДляОтправкиSMS) И Не ЗначениеЗаполнено(ПарольДляОтправкиSMS) Тогда
				ОбщегоНазначения.СообщитьПользователю(
				НСтр("ru = 'Настройка отправки SMS не заполнена. Для заполнения нажмите на гиперссылку ""Настройка отправки SMS""'"),
				,
				"Объект.РежимПодтвержденияЗаписи");
				Отказ = Истина;
			КонецЕсли;
			
		КонецЕсли;
		
		Если Объект.ВариантПодтвержденияНовогоКлиента = Перечисления.ВариантыПодтвержденияНовогоКлиента.ПоEmail Тогда
			Если НЕ РаботаСПочтовымиСообщениями.УчетнаяЗаписьНастроена(РаботаСПочтовымиСообщениями.СистемнаяУчетнаяЗапись()) Тогда
				ОбщегоНазначения.СообщитьПользователю(
				НСтр("ru = 'Настройка системной учетной записи не заполнена. Для заполнения нажмите на гиперссылку ""Настройка системной учетной записи""'"),
				,
				"Объект.РежимПодтвержденияЗаписи");
				Отказ = Истина;
			КонецЕсли;
		КонецЕсли;
		
	КонецЕсли;
	
	Если Объект.ИспользоватьЗаказТоваров Тогда
		
		// ЯНДЕКС.КАССА
		Если Объект.ИспользоватьЯндексКассу Тогда
			
			ПараметрыЯндексКассы = ПараметрыИспользованияЯндексКассы();
			
			Если НЕ ПараметрыЯндексКассы.ОпцияВключена ИЛИ НЕ ПараметрыЯндексКассы.ЕстьНастройкиПодключения Тогда
				ТекстСообщения = НСтр("ru = 'Установлена опция ""Использовать ЮKassa"" и не создано ни одной настройки подключения'");
				ОбщегоНазначения.СообщитьПользователю(ТекстСообщения, , "Объект.ИспользоватьЯндексКассу", , Отказ);
			КонецЕсли;
			
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(Объект.ВидЦенТоваров) Тогда
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Поле ""Вид цен товаров"" не заполнено'"),
			,
			"Объект.ВидЦенТоваров");
			Отказ = Истина;
		КонецЕсли;
		
		Если ИспользоватьВидыЗаказовПокупателей И Не ЗначениеЗаполнено(Объект.ВидЗаказаПокупателя) Тогда
			
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Поле ""Вид заказа покупателя"" не заполнено'"),
			,
			"Объект.ВидЗаказаПокупателя");
			Отказ = Истина;
			
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(Объект.СостояниеЗаказаПокупателя) Тогда
			
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Поле ""Состояние заказа покупателя"" не заполнено'"),
			,
			"Объект.СостояниеЗаказаПокупателя");
			Отказ = Истина;
			
		КонецЕсли;
		
		Если Не Объект.ИспользоватьДоставкуКурьером И Не Объект.ИспользоватьДоставкуПоПочте И Не Объект.ИспользоватьДоставкуВПунктыВыдачиЗаказа Тогда
		
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Укажите хотя бы один способ доставки'"),
			,
			"Объект.ИспользоватьДоставкуКурьером");
			Отказ = Истина;
		
		КонецЕсли;
		
		Если Объект.ИспользоватьДоставкуВПунктыВыдачиЗаказа И Не ЗначениеЗаполнено(ПунктВыдачиЗаказа) Тогда
			
			ОбщегоНазначения.СообщитьПользователю(
			НСтр("ru = 'Поле ""Пункт выдачи заказа"" не заполнено'"),
			,
			"ПунктВыдачиЗаказа");
			Отказ = Истина;
			
		КонецЕсли;
		
	КонецЕсли;
	
	
КонецПроцедуры

&НаКлиенте
Процедура ПослеЗаписи(ПараметрыЗаписи)
	
	Если НадоОпубликоватьНастройки Тогда
		НачатьПубликациюМЛК();
		НадоОпубликоватьНастройки = Ложь;
	КонецЕсли;
	
