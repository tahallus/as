	Элементы.СтраницыНовостногоЦентра.ТекущаяСтраница = Элементы.СтраницаУправлениеБаннерами;
	
	УстановитьПредпросмотрБаннера();
	
КонецПроцедуры

&НаКлиенте
Процедура НовостиВыбор(Элемент, ВыбраннаяСтрока, Поле, СтандартнаяОбработка)
	
	СтандартнаяОбработка = Ложь;
	
	Если Поле.Имя = "НовостиСсылка" Тогда
		
		Оповещение = Новый ОписаниеОповещения("ИзменениеНовостиЗавершение", ЭтотОбъект, Элемент.ТекущиеДанные.Ссылка);
		ОткрытьФорму("Справочник.НовостиМЛК.Форма.ФормаЭлемента", Новый Структура("Ключ", ВыбраннаяСтрока),ЭтотОбъект,,,,Оповещение);
		
	ИначеЕсли Поле.Имя = "НовостиУдалить" Тогда
		
		Оповещение = Новый ОписаниеОповещения("УдалениеНовостиЗавершение", ЭтотОбъект, Элемент.ТекущиеДанные.Ссылка);
		ТекстСообщения = НСтр("ru = 'Удалить новость у пользователей мобильного приложения?'");
		ПоказатьВопрос(Оповещение, ТекстСообщения, РежимДиалогаВопрос.ДаНет);
		
	ИначеЕсли Поле.Имя = "НовостиИзменить" Тогда
		Оповещение = Новый ОписаниеОповещения("ИзменениеНовостиЗавершение", ЭтотОбъект, Элемент.ТекущиеДанные.Ссылка);
		ОткрытьФорму("Справочник.НовостиМЛК.Форма.ФормаЭлемента", Новый Структура("Ключ", Элемент.ТекущиеДанные.Ссылка),ЭтотОбъект,,,,Оповещение);
	КонецЕсли;
	
КонецПроцедуры

