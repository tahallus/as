&НаСервере
Функция ПолучитьПорядокЗаписи()
	
	ПорядокЗаписи = Новый Массив;
	
	Для Счетчик = 1 По 3 Цикл
		
		Если Число(Прав(Элементы.ПолеВыбора1.Родитель.Имя, 1)) = Счетчик Тогда
			ПорядокЗаписи.Добавить("Ресурс");
		ИначеЕсли Число(Прав(Элементы.ПолеВыбора2.Родитель.Имя, 1)) = Счетчик Тогда
			ПорядокЗаписи.Добавить("Услуги");
		ИначеЕсли Число(Прав(Элементы.ПолеВыбора3.Родитель.Имя, 1)) = Счетчик Тогда
			ПорядокЗаписи.Добавить("ДатаИВремя");
		КонецЕсли;
		
	КонецЦикла;
	
	Возврат ПорядокЗаписи;
	
КонецФункции

&НаСервере
Процедура УстановитьСценарийЗаписи()
	
	Если ЗначениеЗаполнено(Объект.Ссылка) Тогда
		
		Запрос = Новый Запрос;
		Запрос.Текст = 
		"ВЫБРАТЬ
		|	НастройкиПубликацииМЛК.ПорядокЗаписи КАК ПорядокЗаписи
		|ИЗ
		|	Справочник.НастройкиПубликацииМЛК КАК НастройкиПубликацииМЛК
		|ГДЕ
		|	НастройкиПубликацииМЛК.Ссылка = &Ссылка";
		
		Запрос.УстановитьПараметр("Ссылка", Объект.Ссылка);
		
		РезультатЗапроса = Запрос.Выполнить();
		
		ВыборкаДетальныеЗаписи = РезультатЗапроса.Выбрать();
		
		Если ВыборкаДетальныеЗаписи.Следующий() Тогда
			ПорядокЗаписи = ВыборкаДетальныеЗаписи.ПорядокЗаписи.Получить();
		КонецЕсли;
		
		Если ПорядокЗаписи = Неопределено Тогда
			Объект.ИспользоватьВыборРесурса = Истина;
		Иначе
			
			Для Счетчик = 0 По 2 Цикл
				
				Если ПорядокЗаписи[Счетчик] = "Ресурс" Тогда
					ЭлементПереноса = Элементы.ПолеВыбора1;
				ИначеЕсли ПорядокЗаписи[Счетчик] = "Услуги" Тогда
					ЭлементПереноса = Элементы.ПолеВыбора2;
				ИначеЕсли ПорядокЗаписи[Счетчик] = "ДатаИВремя" Тогда
					ЭлементПереноса = Элементы.ПолеВыбора3;
				КонецЕсли;
				
				Элементы.Переместить(ЭлементПереноса, Элементы["ГруппаПолеВыбора" + Строка(Счетчик + 1)], Элементы["ГруппаСтрелки" + Строка(Счетчик + 1)]);
				
			КонецЦикла;
			
		КонецЕсли;
		
	Иначе
		Элементы.РежимЗаполненияРесурса.Видимость = Ложь;
		РежимЗаполненияРесурса = Перечисления.РежимыЗаполненияРесурса.ПоМенееЗагруженному;
	КонецЕсли;
	
	Если Объект.ИспользоватьВыборРесурса Тогда
		Элементы.РежимЗаполненияРесурса.Видимость = Ложь;
	КонецЕсли;
	
КонецПроцедуры

&НаСервере
Процедура СформироватьСценарийЗаписи(Знач ТекущийОбъект)
	
	ПорядокЗаписи = ПолучитьПорядокЗаписи();
	
	ТекущийОбъект.ПорядокЗаписи = Новый ХранилищеЗначения(ПорядокЗаписи, Новый СжатиеДанных(9));
	
КонецПроцедуры

&НаСервере
Процедура УстановитьКодКабинета()
	
	КодКомпании = Неопределено;
	
	Если ТекущиеНастройкиКабинетКлиента = Неопределено Тогда
		Возврат;
	КонецЕсли;
	
	Если Не ПриложениеКабинетКлиентаПодключено(ТекущиеНастройкиКабинетКлиента) Тогда
		Возврат;
	КонецЕсли;
	
	Маска = "";
	КоличествоСимволовВКоде = СтрДлина(ТекущиеНастройкиКабинетКлиента.НомерОбласти);
	Для Счетчик = 1 По КоличествоСимволовВКоде Цикл
		Если Счетчик%4 <> 0 Тогда
			Маска = Маска + "9";
		Иначе
			Если Счетчик = КоличествоСимволовВКоде Тогда
				Продолжить;
			КонецЕсли;
			Маска = Маска + "-";
		КонецЕсли;
	КонецЦикла;
	
	Элементы.КодКомпании.Маска = Маска;
	КодКомпании = ТекущиеНастройкиКабинетКлиента.НомерОбласти;
	
КонецПроцедуры

&НаСервере
Процедура УстановитьQRКодКабинета()
	
