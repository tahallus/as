&НаКлиенте
Процедура УдалитьБаннер(Команда)
	
	НомерБаннера = Прав(ЭтаФорма.ТекущийЭлемент.Имя, 1);
	
	УдалитьБаннерНаСервере(НомерБаннера);
	
	УстановитьВидимостьПубликацииЕслиНужно(Истина);
	
	УстановитьПредпросмотрБаннера();
	
КонецПроцедуры

&НаСервере
Процедура УдалитьБаннерНаСервере(НомерБаннера)
	
	ТекОбъект = РеквизитФормыВЗначение("Объект");
	
	Баннеры = ТекОбъект.Баннеры.Получить();
	
	Баннеры.Удалить("Баннер" + НомерБаннера);

	БаннерыОбновленнаяСтруктура = Новый структура;
	ОбновленныйНомерБаннера = 1;
	Для каждого Баннер Из Баннеры Цикл
		БаннерыОбновленнаяСтруктура.Вставить("Баннер" + ОбновленныйНомерБаннера, Баннер.Значение);
		ОбновленныйНомерБаннера = ОбновленныйНомерБаннера + 1;
	КонецЦикла;
	
	ТекОбъект.Баннеры = Новый ХранилищеЗначения(БаннерыОбновленнаяСтруктура, Новый СжатиеДанных(9));
	
	ТекОбъект.Записать();
	ЗначениеВДанныеФормы(ТекОбъект, Объект);
	
	Для Счетчик = 1 По 3 Цикл
		
		Элементы["ДекорацияБаннер" + Счетчик].Видимость = Ложь;
		Элементы["АдресБаннера" + Счетчик].Видимость = Ложь;
		
	КонецЦикла;
	
	УстановитьБаннерыНаФорме();
	
КонецПроцедуры

&НаСервере
Процедура УстановитьБаннер(АдресФайлаВоВременномХранилище, НомерБаннера)

	Если АдресФайлаВоВременномХранилище <> Ложь Тогда
		ДвоичныеДанные = ПолучитьИзВременногоХранилища(АдресФайлаВоВременномХранилище);
	КонецЕсли;
	ТекОбъект = РеквизитФормыВЗначение("Объект");
	
	СтруктураСБаннерами = ТекОбъект.Баннеры.Получить();
	
	Если ТипЗнч(СтруктураСБаннерами) <> Тип("Структура") Тогда
		СтруктураСБаннерами = Новый Структура();
	КонецЕсли;
	
	СтруктураСБаннерами.Вставить("Баннер" + Строка(НомерБаннера), ДвоичныеДанные);
	
	ТекОбъект.Баннеры = Новый ХранилищеЗначения(СтруктураСБаннерами, Новый СжатиеДанных(9));
	ТекОбъект.Записать();
	ЗначениеВДанныеФормы(ТекОбъект, Объект);
	
	ЭтаФорма["Баннер" + Строка(НомерБаннера)] = ПоместитьВоВременноеХранилище(ДвоичныеДанные, УникальныйИдентификатор);
	
	
КонецПроцедуры

&НаСервере
Процедура УстановитьБаннерыНаФорме()
	
	Баннеры = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(Объект.Ссылка, "Баннеры").Получить();
	
	Если ТипЗнч(Баннеры) = Тип("Структура") Тогда
		
		Для каждого Баннер Из Баннеры Цикл
			
			НомерБаннера = Прав(Баннер.Ключ,1);
			
			ЭтаФорма["Баннер" + НомерБаннера] = ПоместитьВоВременноеХранилище(Баннер.Значение, УникальныйИдентификатор);
			
			Элементы["ДекорацияБаннер" + НомерБаннера].Видимость = Ложь;
			Элементы["АдресБаннера" + НомерБаннера].Видимость = Истина;
			
			Если Число(НомерБаннера) < 3 Тогда
				Элементы["ДекорацияБаннер" + (Число(НомерБаннера) + 1)].Видимость = Истина;
			КонецЕсли;
			
		КонецЦикла;
		
		Если Баннеры.Количество() = 0 Тогда
			Элементы.ДекорацияБаннер1.Видимость = Истина;
		КонецЕсли;
		
	Иначе
		Элементы.ДекорацияБаннер1.Видимость = Истина;
	КонецЕсли;
	
КонецПроцедуры

&НаСервере
Процедура УстановитьПодсказкиАссистентаВКонструкторе()

	Если Объект.ПодсказкиАссистента <> "" Тогда
		МассивСПоказаннымиПодсказками = КонструкторМобильногоПриложения.ЧтениеJSONВСтруктуру(Объект.ПодсказкиАссистента);
		Если МассивСПоказаннымиПодсказками.Найти("РаботаБаннеров") <> Неопределено Тогда
			 Элементы.ГруппаСтраницыПодсказкаАссистента.Видимость = Ложь;
		КонецЕсли;
	КонецЕсли;

КонецПроцедуры

&НаСервере
Процедура УстановитьПредпросмотрНовостногоЦентра(ПерваяСтраница = Истина)

	НовостьЗаполнена = Истина;
	
	Если ПерваяСтраница Тогда
		Если ЗначениеЗаполнено(ЗаголовокНовости) Или ЗначениеЗаполнено(КраткоеСообщениеНовости) Тогда
			РедактируемыйSVG = Справочники.НастройкиПубликацииМЛК.ПолучитьМакет("ПредпросмотрНовостнойЦентрСписокНовостей").ПолучитьТекст();	
		Иначе
			РедактируемыйSVG = Справочники.НастройкиПубликацииМЛК.ПолучитьМакет("ПредпросмотрНовостнойЦентрПустойСписокНовостей").ПолучитьТекст();
			НовостьЗаполнена = Ложь;
		КонецЕсли;
	Иначе
		РедактируемыйSVG = Справочники.НастройкиПубликацииМЛК.ПолучитьМакет("ПредпросмотрНовостнойЦентрPushУведомление").ПолучитьТекст();
	КонецЕсли;
	
	ТекущаяДата = ТекущаяДатаСеанса();
	
	Если ПерваяСтраница Тогда
		Если НовостьЗаполнена Тогда
			РедактируемыйSVG = СтрЗаменить(РедактируемыйSVG, "%Заголовок", ?(СтрДлина(ЗаголовокНовости) > 28, Лев(ЗаголовокНовости, 28) + " ...", ЗаголовокНовости));
			РедактируемыйSVG = СтрЗаменить(РедактируемыйSVG, "%КраткоеСообщение", ?(СтрДлина(КраткоеСообщениеНовости) > 30, Лев(КраткоеСообщениеНовости, 30) + " ...", КраткоеСообщениеНовости));
			РедактируемыйSVG = СтрЗаменить(РедактируемыйSVG, "%Дата", Формат(ТекущаяДата, "ДЛФ=DD"));
		КонецЕсли;
	Иначе
		
		ДеньПрописью = Строка(Формат((ТекущаяДата), "ДФ=дддд"));
		ДеньПрописью = ВРег(Лев(ДеньПрописью, 1)) + Сред(ДеньПрописью, 2);
		ЧислоИМесяцИГод =  Строка(Формат((ТекущаяДата), "ДЛФ=DD"));
		ЧислоИМесяц = Лев(ЧислоИМесяцИГод, СтрДлина(ЧислоИМесяцИГод)- 8);
		Время = Лев(Прав(Строка(Формат(НачалоМинуты(ТекущаяДата), "ДЛФ=DT")), 8),5);
		
		РедактируемыйSVG = СтрЗаменить(РедактируемыйSVG, "%КраткоеСообщение", ?(СтрДлина(КраткоеСообщениеНовости) > 30, Лев(КраткоеСообщениеНовости, 30) + " ...", КраткоеСообщениеНовости));
		РедактируемыйSVG = СтрЗаменить(РедактируемыйSVG, "%НазваниеКомпании", ?(СтрДлина(Объект.Наименование) > 30, Лев(Объект.Наименование, 30) + " ...", Объект.Наименование));
		РедактируемыйSVG = СтрЗаменить(РедактируемыйSVG, "%ДеньЧислоМесяц", ДеньПрописью + ", " + ЧислоИМесяц);
		РедактируемыйSVG = СтрЗаменить(РедактируемыйSVG, "%Время", Время);
		
	КонецЕсли;
	
	
	УстановитьКартинкуSVGНаФорме(РедактируемыйSVG);

КонецПроцедуры

&НаКлиенте
Процедура УстановитьЦветИДоступностьКнопкиОпубликоватьНовость()
	
	Если НовостьЗаполнена() Тогда
		Элементы.УведомлениеОтправить.Доступность = Истина;
		Элементы.УведомлениеОтправить.ЦветФона = Новый Цвет(255, 225, 0);
	Иначе
		Элементы.УведомлениеОтправить.Доступность = Ложь;
		Элементы.УведомлениеОтправить.ЦветФона = Новый Цвет(160, 160, 160);
		
	КонецЕсли;

КонецПроцедуры

