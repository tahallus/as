		SVG = SVG + Символы.ПС + "<text style=""cursor: move;"" xml:space=""preserve"" text-anchor=""start"" font-family=""Helvetica, Arial, sans-serif"" font-size=""9"" id=""svg_3"" y=""117"" x=""88.426143"" stroke-width=""0"" stroke=""#000"" fill=""#009646"">Контакты</text>
		|<text xml:space=""preserve"" text-anchor=""start"" font-family=""Helvetica, Arial, sans-serif"" font-size=""9"" id=""svg_4"" y=""117"" x=""137.537248"" stroke-width=""0"" stroke=""#000"" fill=""#000000"">Информация</text>";
		
	ИначеЕсли ВыделеннаяСтраница = "Информация" Тогда
		SVG = SVG + Символы.ПС + "<text style=""cursor: move;"" xml:space=""preserve"" text-anchor=""start"" font-family=""Helvetica, Arial, sans-serif"" font-size=""9"" id=""svg_3"" y=""117"" x=""88.426143"" stroke-width=""0"" stroke=""#000"" fill=""#000000"">Контакты</text>
		|<text xml:space=""preserve"" text-anchor=""start"" font-family=""Helvetica, Arial, sans-serif"" font-size=""9"" id=""svg_4"" y=""117"" x=""137.537248"" stroke-width=""0"" stroke=""#000"" fill=""#009646"">Информация</text>";
