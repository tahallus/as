			Слой2 = СтрЗаменить(Слой2, "%ЗаголовокВысота", "192");
			Слой2 = СтрЗаменить(Слой2, "%ЗаголовокШирина", "40");
			Слой2 = СтрЗаменить(Слой2, "%ПодсказкаВысота", "217");
			Слой2 = СтрЗаменить(Слой2, "%ПодсказкаШирина", "17");
			МассивССоединяемымиСтроками.Добавить(Слой2);
		Иначе
			Слой3 = Справочники.НастройкиПубликацииМЛК.ПолучитьМакет("ПредпросмотрНоваяЗапись" + ПорядокЗаписи[Счетчик]).ПолучитьТекст();
			Слой3 = СтрЗаменить(Слой3, "%КартинкаВысота", "242");
			Слой3 = СтрЗаменить(Слой3, "%КартинкаШирина", "17");
			Слой3 = СтрЗаменить(Слой3, "%ЗаголовокВысота", "257");
			Слой3 = СтрЗаменить(Слой3, "%ЗаголовокШирина", "40");
			Слой3 = СтрЗаменить(Слой3, "%ПодсказкаВысота", "282");
			Слой3 = СтрЗаменить(Слой3, "%ПодсказкаШирина", "17");
			МассивССоединяемымиСтроками.Добавить(Слой3);
		КонецЕсли;
		
	КонецЦикла;
	
	
	
КонецПроцедуры

&НаКлиенте
Процедура ОтобразитьСтраницу(ИмяДекорацииВМеню)
	
	Если ИмяДекорацииВМеню = "ВкладкиИНаполнение" Тогда
		Элементы.СтраницыКонструктора.ТекущаяСтраница = Элементы.СтраницаВкладкиМобильногоПриложения;
	КонецЕсли;
	
	Если ИмяДекорацииВМеню = "Главное" Тогда
		Элементы.СтраницыКонструктора.ТекущаяСтраница = Элементы.СтраницаНастройки;
	КонецЕсли;
	
	Если ИмяДекорацииВМеню = "Публикация" Тогда
		Элементы.СтраницыКонструктора.ТекущаяСтраница = Элементы.СтраницаПубликация;
	КонецЕсли;
	
	Если ИмяДекорацииВМеню = "ЦентрУведомлений" Тогда
		Элементы.СтраницыКонструктора.ТекущаяСтраница = Элементы.СтраницаНовостнойЦентр;
	КонецЕсли;
	
	Если ИмяДекорацииВМеню = "ДляРазработчиков" Тогда
		Элементы.СтраницыКонструктора.ТекущаяСтраница = Элементы.СтраницаДляРазработчиков;
	КонецЕсли;
	
