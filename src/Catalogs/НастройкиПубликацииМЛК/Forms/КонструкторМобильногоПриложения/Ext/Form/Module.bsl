&НаСервере
Процедура УстановитьПредпросмотрБаннера(НомерБаннера = 0)

	// Устанавливаем первый баннер предпросмотре
	Если НомерБаннера = 0 И Элементы.АдресБаннера1.Видимость Тогда
		НомерБаннера = 1;
	КонецЕсли;
	
	МассивССоединяемымиСтроками = Новый Массив;
	
	РедактируемыйSVG = Справочники.НастройкиПубликацииМЛК.ПолучитьМакет("ПредпросмотрНовостнойЦентрБаннер").ПолучитьТекст();
	
	Если НомерБаннера = 0 Тогда
		Если ЗначениеЗаполнено(Объект.Ссылка) Тогда
			Логотип = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(Объект.Ссылка, "Логотип").Получить();
			ЛоготипИлиБаннерBase64 = Base64Строка(Логотип);
		Иначе
			ЛоготипИлиБаннерBase64 = "";
		КонецЕсли;
	Иначе
		
		Если ЗначениеЗаполнено(Объект.Ссылка) Тогда
			СтруктураСБаннерами = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(Объект.Ссылка, "Баннеры").Получить();
		Иначе
			СтруктураСБаннерами = Неопределено;
		КонецЕсли;
		
		Если ТипЗнч(СтруктураСБаннерами) = Тип("Структура") И СтруктураСБаннерами.Свойство("Баннер" + Строка(НомерБаннера)) Тогда
			Баннер = СтруктураСБаннерами["Баннер" + Строка(НомерБаннера)]
		Иначе
			Баннер = "";
		КонецЕсли;
		
		ЛоготипИлиБаннерBase64 = Base64Строка(Баннер);
		
	КонецЕсли;
	
	МассивССоединяемымиСтроками.Добавить(СтрЗаменить(РедактируемыйSVG, "%БаннерВBase64", ЛоготипИлиБаннерBase64));
	
	ТемноСерыйКруг = Справочники.НастройкиПубликацииМЛК.ПолучитьМакет("ПредпросмотрНовостнойЦентрПролистываниеТемноСерыйКруг").ПолучитьТекст();
	СерыйКруг = Справочники.НастройкиПубликацииМЛК.ПолучитьМакет("ПредпросмотрНовостнойЦентрПролистываниеСерыйКруг").ПолучитьТекст();
	
	Если Элементы.АдресБаннера3.Видимость Тогда
		
		// 4 круга
		Для Сч = 0 По 3 Цикл
			Если Сч = НомерБаннера Тогда
				ДобавляемыйКруг = ТемноСерыйКруг;
			Иначе
				ДобавляемыйКруг = СерыйКруг;
			КонецЕсли;
			
			Если Сч = 0 Тогда
				ДобавляемыйКруг = СтрЗаменить(ДобавляемыйКруг, "%ПоложениеПоГоризонтали", "180");
			ИначеЕсли Сч = 1 Тогда
				ДобавляемыйКруг = СтрЗаменить(ДобавляемыйКруг, "%ПоложениеПоГоризонтали", "190");
			ИначеЕсли Сч = 2 Тогда
				ДобавляемыйКруг = СтрЗаменить(ДобавляемыйКруг, "%ПоложениеПоГоризонтали", "200");
			Иначе // 3
				ДобавляемыйКруг = СтрЗаменить(ДобавляемыйКруг, "%ПоложениеПоГоризонтали", "210");
			КонецЕсли;
			
			МассивССоединяемымиСтроками.Добавить(ДобавляемыйКруг);
			
		КонецЦикла;
		
	ИначеЕсли Элементы.АдресБаннера2.Видимость Тогда
		
		// 3 круга
		Для Сч = 0 По 2 Цикл
			Если Сч = НомерБаннера Тогда
				ДобавляемыйКруг = ТемноСерыйКруг;
			Иначе
				ДобавляемыйКруг = СерыйКруг;
			КонецЕсли;
			
			Если Сч = 0 Тогда
				ДобавляемыйКруг = СтрЗаменить(ДобавляемыйКруг, "%ПоложениеПоГоризонтали", "185");
			ИначеЕсли Сч = 1 Тогда
				ДобавляемыйКруг = СтрЗаменить(ДобавляемыйКруг, "%ПоложениеПоГоризонтали", "195");
			Иначе // 2
				ДобавляемыйКруг = СтрЗаменить(ДобавляемыйКруг, "%ПоложениеПоГоризонтали", "205");
			КонецЕсли;
			
			МассивССоединяемымиСтроками.Добавить(ДобавляемыйКруг);
			
		КонецЦикла;
		
	ИначеЕсли Элементы.АдресБаннера1.Видимость Тогда
		
		// 2 круга
		Для Сч = 0 По 1 Цикл
			Если Сч = НомерБаннера Тогда
				ДобавляемыйКруг = ТемноСерыйКруг;
			Иначе
				ДобавляемыйКруг = СерыйКруг;
			КонецЕсли;
			
			Если Сч = 0 Тогда
				ДобавляемыйКруг = СтрЗаменить(ДобавляемыйКруг, "%ПоложениеПоГоризонтали", "190");
			Иначе // 2
				ДобавляемыйКруг = СтрЗаменить(ДобавляемыйКруг, "%ПоложениеПоГоризонтали", "200");
			КонецЕсли;
			
			МассивССоединяемымиСтроками.Добавить(ДобавляемыйКруг);
			
		КонецЦикла;
		
	КонецЕсли;
	
	МассивССоединяемымиСтроками.Добавить("</svg>");
	ИтоговоеSVG = СтрСоединить(МассивССоединяемымиСтроками, Символы.ПС);
	
	УстановитьКартинкуSVGНаФорме(ИтоговоеSVG);
	
КонецПроцедуры

