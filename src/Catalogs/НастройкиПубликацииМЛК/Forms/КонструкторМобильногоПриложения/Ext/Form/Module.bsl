&НаСервереБезКонтекста
Функция ПредставлениеКомандыНастроитьКаталогТоваров(АдресКаталога)
	
	ПредставлениеКоманды = НСтр("ru='Товары к выгрузке'");
	
	Если ЗначениеЗаполнено(АдресКаталога) Тогда
		
		КаталогТоваров = ПолучитьИзВременногоХранилища(АдресКаталога);
		
		Если ТипЗнч(КаталогТоваров) = Тип("ТаблицаЗначений") И КаталогТоваров.Количество() > 0 Тогда
			
			Итератор = 0;
			ПредставлениеКоманды = "";
			
			Для каждого СтрокаТовара Из КаталогТоваров Цикл
				
				Если СтрокаТовара.ЭтоГруппа Тогда
					Продолжить;
				КонецЕсли;
				
				ПредставлениеКоманды = ПредставлениеКоманды + СтрокаТовара.ГруппаКаталога;
				
				Если Итератор >= 1 ИЛИ КаталогТоваров.Количество() = Итератор + 1 Тогда
					Прервать;
				КонецЕсли;
				
				ПредставлениеКоманды = ПредставлениеКоманды + ", ";
				Итератор = Итератор + 1;
				
			КонецЦикла;
			
			Если СтрДлина(ПредставлениеКоманды) > 90 Тогда
				ПредставлениеКоманды = Прав(ПредставлениеКоманды, 90);
			КонецЕсли;
			
			ПредставлениеКоманды = СтрЗаменить(ПредставлениеКоманды, " ", Символы.НПП);
			ПредставлениеКоманды = ПредставлениеКоманды +  "...";
