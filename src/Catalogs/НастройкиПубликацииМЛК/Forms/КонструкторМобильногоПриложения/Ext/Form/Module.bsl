	КонецЕсли;
	
	QRСтрока = "sbmcs" + ";" + ТекущиеНастройкиКабинетКлиента.АдресПриложения;
	ДанныеQRКода = ГенерацияШтрихкода.ДанныеQRКода(QRСтрока, 0, 190);
	QRКод = ПоместитьВоВременноеХранилище(ДанныеQRКода, УникальныйИдентификатор);
	
