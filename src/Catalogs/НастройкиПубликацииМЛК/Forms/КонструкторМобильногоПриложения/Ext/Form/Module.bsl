	ОписаниеОповещения = Новый ОписаниеОповещения("ВыбратьРеквизитыЗавершение", ЭтотОбъект);
	ОткрытьФорму("Справочник.НастройкиПубликацииМЛК.Форма.ФормаВыбораРеквизитов", Новый Структура("НастройкиВыгрузкиРеквизитов", Объект.НастройкиВыгрузкиРеквизитов), ЭтотОбъект,,,,ОписаниеОповещения);
	
КонецПроцедуры

&НаКлиенте
Процедура ВыбратьРеквизитыЗавершение(ВыбранныйЭлемент, ДополнительныеПараметры) Экспорт
	
	Если ВыбранныйЭлемент = Неопределено Тогда
		Возврат;
	КонецЕсли;
	
	Если ВыбранныйЭлемент.Количество()=0 Тогда
		Объект.НастройкиВыгрузкиРеквизитов = "";
	Иначе
		Объект.НастройкиВыгрузкиРеквизитов = КонструкторМобильногоПриложения.ЗаписьJSONВСтруктуру(ВыбранныйЭлемент);
	КонецЕсли;
	
	НастройкиВыгрузкиРеквизитовПредставление = НастройкиВыгрузкиРеквизитовПредставление(Объект.НастройкиВыгрузкиРеквизитов);
	Модифицированность = Истина;
	
КонецПроцедуры

&НаСервере
Функция НастройкиВыгрузкиРеквизитовПредставление(НастройкиВыгрузкиРеквизитов)
	
	СтрокаПредставление = "";
	Если НастройкиВыгрузкиРеквизитов = "" Тогда
		СтрокаПредставление = "Дополнительные реквизиты";
	Иначе
		СтруктураНастройки = КонструкторМобильногоПриложения.ЧтениеJSONВСтруктуру(НастройкиВыгрузкиРеквизитов);
		Для каждого СтрокаНастроек Из СтруктураНастройки Цикл
			СтрокаПредставление = СтрокаПредставление + СтрокаНастроек.Ключ + ", ";
		КонецЦикла;
		СтрокаПредставление = Лев(СтрокаПредставление, СтрДлина(СтрокаПредставление)-2);
	КонецЕсли;
	
	Возврат СтрокаПредставление;
	
КонецФункции

&НаКлиенте
Процедура НастроитьКаталог(Команда)
	
	СтруктураПараметров = Новый Структура;
	СтруктураПараметров.Вставить("АдресКаталогаТоваров", АдресКаталогаТоваров);
	СтруктураПараметров.Вставить("ОтборГруппыКатегорииНоменклатуры", Объект.ОтборГруппыКатегорииНоменклатуры);
	СтруктураПараметров.Вставить("ВидЦен",  Объект.ВидЦенТоваров);
	СтруктураПараметров.Вставить("НастройкиВыгрузкиРеквизитов",  Объект.НастройкиВыгрузкиРеквизитов);
	СтруктураПараметров.Вставить("КодКомпании", КодКомпании);
	СтруктураПараметров.Вставить("АдресИндивидуальныхЦен", АдресИндивидуальныхЦен);
	СтруктураПараметров.Вставить("АдресПриложения", ?(ТекущиеНастройкиКабинетКлиента <> Неопределено, ТекущиеНастройкиКабинетКлиента.АдресПриложения, ""));
	
	ОписаниеОповещения = Новый ОписаниеОповещения("НастройкаСтруктурыКаталогаЗавершение", ЭтотОбъект);

	ОткрытьФорму("Справочник.НастройкиПубликацииМЛК.Форма.ФормаВыборНоменклатуры", СтруктураПараметров,ЭтотОбъект,,,,ОписаниеОповещения);
	
	ВерсияЗаказТоваровИзменяемая = ВерсияЗаказТоваровИзменяемая + 1;
	
КонецПроцедуры

&НаСервере
Процедура ПолучитьАдресКаталогаТоваров()
	
	Запрос = Новый Запрос;
	Запрос.Текст = 
		"ВЫБРАТЬ
		|	НастройкиПубликацииМЛК.КаталогТоваров КАК КаталогТоваров
		|ИЗ
		|	Справочник.НастройкиПубликацииМЛК КАК НастройкиПубликацииМЛК
		|ГДЕ
		|	НастройкиПубликацииМЛК.Ссылка = &Ссылка";
	
	Запрос.УстановитьПараметр("Ссылка", Объект.Ссылка);
	
	РезультатЗапроса = Запрос.Выполнить();
	
	ВыборкаДетальныеЗаписи = РезультатЗапроса.Выбрать();
	
	Если ВыборкаДетальныеЗаписи.Следующий() Тогда
		
		КаталогТоваров = ВыборкаДетальныеЗаписи.КаталогТоваров.Получить();
		
		Если ТипЗнч(КаталогТоваров) = Тип("ТаблицаЗначений") Тогда
			АдресКаталогаТоваров = ПоместитьВоВременноеХранилище(КаталогТоваров, Новый УникальныйИдентификатор);;
		Иначе
			АдресКаталогаТоваров = "";
		КонецЕсли;
	Иначе
		АдресКаталогаТоваров = "";
	КонецЕсли;
	
КонецПроцедуры // ()

&НаСервере
Процедура СформироватьЭлементыФормыСкладов()
	
	Ид = 0;
	Для каждого ТекСтрока Из ВсеСклады Цикл
		Переключатель = Элементы.Добавить("Склад" + Ид, Тип("ПолеФормы"), Элементы.ГруппаСклады);
		Переключатель.Вид = ВидПоляФормы.ПолеПереключателя;
		Переключатель.ПутьКДанным = "ВсеСклады[" + Ид + "].Выбран";
		Переключатель.Заголовок = ВсеСклады[Ид].Склад;
		Переключатель.ПоложениеЗаголовка = ПоложениеЗаголовкаЭлементаФормы.Право;
		Переключатель.УстановитьДействие("ПриИзменении", "ПриИзмененииСклад");
		Ид = Ид + 1;
	КонецЦикла;
	
КонецПроцедуры

&НаСервере
Процедура ЗаполнитьСписокСкладов()
	
	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ
	|	ЛОЖЬ КАК Выбран,
	|	СтруктурныеЕдиницы.Ссылка КАК Склад
	|ИЗ
	|	Справочник.СтруктурныеЕдиницы КАК СтруктурныеЕдиницы
	|ГДЕ
	|	СтруктурныеЕдиницы.ТипСтруктурнойЕдиницы = ЗНАЧЕНИЕ(Перечисление.ТипыСтруктурныхЕдиниц.Склад)";
	
	ТаблицаСкладов = Запрос.Выполнить().Выгрузить();
	ВсеСклады.Загрузить(ТаблицаСкладов);
	
	Для каждого ТекСтрока Из Объект.Склады Цикл
		НайденныеСтроки = ВсеСклады.НайтиСтроки(Новый Структура("Склад", ТекСтрока.Склад));
		Если НайденныеСтроки.Количество() > 0 Тогда
			НайденныеСтроки[0].Выбран = Истина;
		КонецЕсли;
	КонецЦикла;
	
КонецПроцедуры

&НаКлиенте
Процедура ПриИзмененииСклад(Элемент)
	
	Объект.Склады.Очистить();
	
	Для каждого ТекСклад Из ВсеСклады Цикл
		Если ТекСклад.Выбран Тогда
			НоваяСтрока = Объект.Склады.Добавить();
			НоваяСтрока.Склад = ТекСклад.Склад;
		КонецЕсли;
	КонецЦикла;
	
	СформироватьЗаголовокГруппыСкладов();
	УстановитьВидимостьПубликацииЕслиНужно(Истина);
	
КонецПроцедуры

